        <dct:description><![CDATA["number":3,"values":[{"value":1},{"value":2},{"value":3},{"value":4},{"value":5},{"value":6},{"value":7},{"value":8},{"value":9},{"value":10},{"value":11},{"value":12},{"value":13},{"value":14},{"value":15},{"id":"minus"}],"gen":"function(){var a=Math.floor(Math.random()*10)+6,b,ret=\\\"<div style='width:1em;height:1em;margin-left:.2em;font-size:5em;'><svg xmlns='http://www.w3.org/2000/svg' width='100%' height='100%' viewBox='0 0 48 48'><def><style>text {font-size:16px;text-anchor:middle;font-weight:bold;fill:#332920;}</style></def>\\\";do { b=Math.floor(Math.random()*15)+1} while (a<=b); ret+=\\\"<text transform='translate(24,24)'>\\\"+a+\\\"-\\\"+b+\\\"</text></svg></div>\\\"; return {\\\"figure\\\":{\\\"content\\\":ret},\\\"result\\\":a+\\\"-\\\"+b } }","errratio":2]]></dct:description>
