    <rdf:Description>
        <dct:identifier>005o</dct:identifier>
        <dct:title xml:lang="fr-FR">Ecrire une opération arithmétique aléatoire sous la forme d'une arborescence [addition, multiplication, soustraction].</dct:title>
        <dct:description><![CDATA["gg":["+","×","-"],"number":5,"values":[{"va":1},{"va":2},{"va":3},{"va":4},{"va":5},{"va":6},{"va":7},{"va":8},{"va":9},{"va":10},{"va":11},{"va":12},{"va":13},{"va":14},{"va":15},{"va":"+"},{"va":"×"},{"va":"-"}],"gen":"function($this,_s,_i){var o=Math.floor(Math.random()*_s.gg.length),a=Math.floor(Math.random()*15)+1,b=Math.floor(Math.random()*15)+1,ret=\\\"<div style='width:1em;height:1em;margin-left:.2em;font-size:5em;'><svg xmlns='http://www.w3.org/2000/svg' width='100%' height='100%' viewBox='0 0 48 48' id='mtfig'><def><style>svg#mtfig text {font-size:16px;text-anchor:middle;font-weight:bold;fill:#332920;}</style></def>\\\"; ret+=\\\"<text transform='translate(24,24)'>\\\"+a+_s.gg[o]+b+\\\"</text></svg></div>\\\"; return {\\\"figure\\\":{\\\"content\\\":ret},\\\"result\\\":a+_s.gg[o]+b } }","errratio":2]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"[b]Recopiez[/b] l'opération ci-dessus."]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>mathematics</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
