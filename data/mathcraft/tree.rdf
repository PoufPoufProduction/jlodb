    <rdf:Description>
        <dct:identifier>005o</dct:identifier>
        <dct:title xml:lang="fr-FR">Ecrire une opération arithmétique aléatoire sous la forme d'une arborescence [addition, multiplication, soustraction].</dct:title>
        <dct:description><![CDATA["number":5,"values":[{"value":1},{"value":2},{"value":3},{"value":4},{"value":5},{"value":6},{"value":7},{"value":8},{"value":9},{"value":10},{"value":11},{"value":12},{"value":13},{"value":14},{"value":15},{"id":"plus"},{"id":"mult"},{"id":"minus"}],"gen":"function(){var op=[['+','+'],['*','×'],['-','-']],o=Math.floor(Math.random()*3),a=Math.floor(Math.random()*(o==2?10:15))+(o==2?6:1),b,ret=\\\"<div style='width:1em;height:1em;margin-left:.2em;font-size:5em;'><svg xmlns='http://www.w3.org/2000/svg' width='100%' height='100%' viewBox='0 0 48 48'><def><style>text {font-size:16px;text-anchor:middle;font-weight:bold;fill:#332920;}</style></def>\\\";do { b=Math.floor(Math.random()*15)+1} while (o==2&&a<b); ret+=\\\"<text transform='translate(24,24)'>\\\"+a+op[o][1]+b+\\\"</text></svg></div>\\\"; return {\\\"figure\\\":{\\\"content\\\":ret},\\\"result\\\":a+op[o][0]+b } }","errratio":2]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"[b]Recopiez[/b] l'opération ci-dessus."]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>mathematics</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
