    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Ecrire sous la forme d'un arbre l'opération 2+1.</dct:title>
        <dct:description><![CDATA["figure":{"url":"res/img/background/mathcraft01.svg"},"values":[{"value":1},{"value":2},{"id":"plus"}],"result":"1+2","onlyone":true,"err":5]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Représentez l'opération suivante :[/b] [math]<mrow><mn>1</mn><mo>+</mo><mn>2</mn></mrow>[/math]","Faites glisser l'opérateur [red][b]+[/b][/red] vers l'éditeur, puis remplissez les 2 cases vides avec [red][b]1[/b][/red] et [red][b]2[/b][/red]. [b]Validez[/b]."]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>mathematics</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:type>1</dct:type>
        <dct:alternative>024</dct:alternative>
    </rdf:Description>

