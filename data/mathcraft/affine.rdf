<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:dct="http://purl.org/dc/terms/">

    <rdf:Description>
        <dct:identifier>p001</dct:identifier>
        <dct:title xml:lang="fr-FR">Ecrire le parallélisme de 2 droites.</dct:title>
        <dct:description><![CDATA["errratio":5,"data":[{"figure":{"content":"<path d='M 0,200 L 640,50' class='l'/><path d='M 0,300 L 640,150' class='l'/><text transform='translate(100,150)'>d1</text><text transform='translate(150,300)'>d2</text>"},"values":[{"type":"v","value":"d1"},{"type":"v","value":"d2"},{"type":"op","value":"par"}],"result":"par(d1,d2)"}],"glossary":false]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":[["Soient [b]d1[/b] et [b]d2[/b] deux droites qui ne se coupent en aucun point. [b]Ecrivez la relation qui lie les 2 droites[/b].","","[strong]L'activité MathCraft propose une écriture scientifique par arborescence.[/strong]","Pour répondre à l'exercice, commencez par faire glisser l'opérateur [b]'parallèle'[/b] dans la zone d'édition, puis remplissez les deux 'feuilles' de l'arbre ainsi créé par chacune des deux valeurs proposées."]]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>geometry</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>p001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>p002</dct:identifier>
        <dct:title xml:lang="fr-FR">Identifier 2 droites parallèles.</dct:title>
        <dct:description><![CDATA["errratio":5,"data":[{"figure":{"content":"<path d='M 300,0 L 500,480' class='l'/><path d='M 100,0 L 300,480' class='l'/><path d='M 0,200 L 640,320' class='l'/><text transform='translate(10,185)'>d1</text><text transform='translate(70,30)'>d2</text><text transform='translate(270,30)'>d3</text>"},"values":[{"type":"v","value":"d1"},{"type":"v","value":"d2"},{"type":"v","value":"d3"},{"type":"op","value":"par"}],"result":"par(d3,d2)"}],"glossary":false]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":[["Identifiez grâce à la figure ci-dessus, [b]les deux droites parallèles[/b]."]]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>geometry</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>p001</dct:alternative>
