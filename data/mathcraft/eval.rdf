    <rdf:Description>
        <dct:identifier>002o</dct:identifier>
        <dct:title xml:lang="fr-FR">Exécuter une multiplication afin d'obtenir le résultat demandé.</dct:title>
        <dct:description><![CDATA["number":3,"values":[{"value":1},{"value":2},{"value":3},{"value":4},{"value":5},{"value":6},{"value":7},{"value":8},{"value":9},{"value":10},{"id":"mult"},{"id":"eval"}],"gen":"function(){var a=Math.floor(Math.random()*9)+2,b,ret=\\\"<div style='width:1em;height:1em;margin-left:.2em;font-size:5em;'><svg xmlns='http://www.w3.org/2000/svg' width='100%' height='100%' viewBox='0 0 48 48'><def><style>text {font-size:30px;text-anchor:middle;font-weight:bold;fill:#332920;}</style></def>\\\";do { b=Math.floor(Math.random()*9)+2} while (b*a<=10); ret+=\\\"<text transform='translate(24,35)'>\\\"+(a*b)+\\\"</text></svg></div>\\\"; return {\\\"figure\\\":{\\\"content\\\":ret},\\\"result\\\":a*b } }","errratio":5,"glossary":false,"class":"ex"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"A l'aide de l'action d'[red]évaluation[/red] et de la bonne multiplication, [b]affichez[/b] la valeur demandée."]]></dct:description>
