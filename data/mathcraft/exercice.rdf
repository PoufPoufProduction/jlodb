        <dct:identifier>003</dct:identifier>
        <dct:title xml:lang="fr-FR">Comprendre un énoncé simple (3+4+1).</dct:title>
        <dct:description><![CDATA["values":[{"value":1},{"value":2},{"value":3},{"value":4},{"value":5},{"id":"plus"}],"result":"3+4+1","class":"nofig","err":5]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Le train comporte [red]3 wagons[/red] de 1ère classe, [red]4 wagons[/red] de 2ème classe et d'[red]1 wagon[/red] restaurant.","","De [b]combien de wagons[/b] est composé le train ?","","[blue]Représentez le résultat sous la forme d'une opération correspondant à l'énoncé.[/blue]"]]]></dct:description>
