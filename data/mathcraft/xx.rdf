        <dct:title xml:lang="fr-FR">Identifier un triangle rectangle issu d'une médiatrice.</dct:title>
        <dct:description><![CDATA["data":[{"figure":{"content":"<g transform='translate(320,260)'><g transform='rotate(280)'><path d='M 0,-200 L 0,300' class='l d'/><path d='M -200,-200 L 200,-200 L 0,300 L -200,-200 z' class='l'/><circle cx='-200' cy='-200' r='5' class='p'/><circle cx='200' cy='-200' r='5' class='p'/><circle cx='0' cy='-200' r='5' class='p'/><circle cx='0' cy='300' r='5' class='p'/></g></g><text transform='translate(610,300)'>A</text><text transform='translate(60,420)'>C</text><text transform='translate(130,260)' class='dd'>M</text><text transform='translate(120,40)'>B</text>"},"values":[{"type":"tree","abstract":"#1","value":"eq","children":[{"type":"v","value":"AB"},{"type":"v","value":"AC"}]},{"type":"tree","abstract":"#2","value":"middle","children":[{"type":"v","value":"M"},{"type":"v","subtype":"segment","value":"BC"}]},{"type":"action","value":"replace"}],"result":["rtriangle(MAB,M)"]}],"class":"in ex","glossary":true,"modif":false,"a":["t2","b11","b12","mid2d","mid2in","mids2med","med2perp","rtriangle","inline"]]]></dct:description>
