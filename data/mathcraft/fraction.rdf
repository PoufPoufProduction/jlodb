        <dct:description><![CDATA["number":5,"values":[{"value":1},{"value":2},{"value":3},{"value":4},{"value":5},{"value":6},{"value":7},{"value":8},{"value":9},{"value":10},{"value":11},{"value":12},{"value":13},{"value":14},{"value":15},{"value":16},{"id":"div"}],"gen":"function(){var des=[2,3,4,5,6,8,9,12,16],de=des[Math.floor(Math.random()*des.length)],num=Math.floor(Math.random()*de)+1,ret=\\\"<div style='width:1em;height:1em;margin-left:.2em;font-size:5em;'><svg xmlns='http://www.w3.org/2000/svg' width='100%' height='100%' viewBox='0 0 48 48'><def><style>.s {fill:none;stroke:black;stroke-width:0.5;}.f{fill:red;}</style></def>\\\";if (num==de) { ret+=\\\"<circle cx='24' cy='24' r='22' class='f'/>\\\"; } else { ret+=\\\"<path d='M24,24 v-22 a22,22 0 \\\"+(num>de/2?1:0)+\\\",1 \\\"+(22*Math.sin(2*Math.PI*num/de))+\\\",\\\"+(22-22*Math.cos(2*Math.PI*num/de))+\\\" z' class='f'/>\\\"; } for (var i=0;i<de;i++) { ret+=\\\"<line x1='24' y1='24' x2='\\\"+(24+22*Math.sin(2*Math.PI*i/de))+\\\"' y2='\\\"+(24-22*Math.cos(2*Math.PI*i/de))+\\\"' class='s'/>\\\"; } ret+=\\\"<circle cx='24' cy='24' r='22' class='s'/></svg></div>\\\"; return {'figure':{'content':ret},'result':num+'/'+de } }","errratio":2]]></dct:description>
