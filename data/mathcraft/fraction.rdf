     <rdf:Description>
        <dct:identifier>fp1</dct:identifier>
        <dct:title xml:lang="fr-FR">Représenter une valeur donnée sous la forme d'une fraction décimale.</dct:title>
        <dct:description><![CDATA["number":5,"values":[{"value":3},{"value":18},{"value":72},{"value":99},{"value":123},{"value":301},{"value":499},{"value":807},{"value":999},{"value":1278},{"value":1604},{"value":1999},{"value":3005},{"value":4205},{"value":10},{"value":100},{"value":1000},{"id":"div"}],"gen":"function(){var nums=[3,18,72,99,123,301,499,807,999,1278,1604,1999,3005,4205],num=nums[Math.floor(Math.random()*nums.length)],des=[10,100,1000],de=des[Math.floor(Math.random()*des.length)]; return {'figure':{'type':'txt','content':'[div2]'+(num/de)+'[/div]'},'result':num+'/'+de } }","errratio":2]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Représentez [b]sous la forme d'une fraction[/b] la valeur ci-dessus."]]></dct:description>
