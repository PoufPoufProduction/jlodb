     <rdf:Description>
        <dct:identifier>fp2</dct:identifier>
        <dct:title xml:lang="fr-FR">Représenter une valeur donnée sous la forme d'une décomposition en fractions décimales.</dct:title>
        <dct:description><![CDATA["number":3,"values":[{"value":2},{"value":3},{"value":4},{"value":5},{"value":6},{"value":7},{"value":8},{"value":9},{"value":10},{"value":100},{"value":1000},{"id":"plus"},{"id":"div"},{"id":"mult"}],"gen":"function(){var f=[100,10,1,0.1,0.01,0.001],r=['*100','*10','','/10','/100','/1000'],val,ret,nb; do { nb=0;val=0; ret=[]; for (var i=0; i<f.length; i++) { if (Math.random()>0.5) { var d=Math.floor(Math.random()*8)+2; nb++; val+=d*f[i]; ret.push(d+r[i]); } } } while(val==0 || nb>4 || nb<2); return {'figure':{'type':'txt','content':'[div2]'+val+'[/div]'},'result':ret.join('+') } }","errratio":2]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Représentez [b]sous la forme d'une décomposition de fractions[/b] la valeur ci-dessus."]]></dct:description>
