    <!-- FORWARD: a[a-z] -->

    <rdf:Description>
        <dct:identifier>1aa</dct:identifier>
        <dct:title xml:lang="fr-FR">A l'aide de l'opérateur [avancer] (x1), programmer un robot afin qu'il atteigne la case d'arrivée.</dct:title>
        <dct:description><![CDATA["board":[[401,552],[1,2]],"robots":[{"origin":[0,1,0],"code":[1,0,0],"actions":["f01"]}],"padding":10]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"course":["Dans cet exercice, le parcours à effectuer par le robot\\npour arriver sur sa case d'arrivée est très simple puisqu'il\\nlui suffit d'avancer d'une case.","Faites glisser l'action 'AVANCER D'UNE CASE' de la zone\\nde dépôt, où sont stockées toutes les actions disponibles,\\nvers la zone MAIN qui représente le programme principal.\\n\\nCliquez sur le bouton de 'lecture' pour exécuter le\\nprogramme ainsi créé."]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>4d</dct:alternative>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>1ab</dct:identifier>
        <dct:title xml:lang="fr-FR">A l'aide de l'opérateur [avancer] (x2), programmer un robot afin qu'il atteigne la case d'arrivée.</dct:title>
        <dct:description><![CDATA["board":[[455,1],[401,1],[455,2]],"robots":[{"origin":[1,0,1],"code":[2,0,0],"actions":["f01","f01"]}]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"course":["Ici, le robot doit avancer de 2 cases. Or il n'existe pas d'action\\nélémentaire permettant ce genre d'opération. La seule action\\ndisponible est 'AVANCER D'UNE CASE'.\\n\\nHeureusement, il est tout à fait possible d'enchaîner deux fois\\ncette même action."]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>4d</dct:alternative>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
    </rdf:Description>

