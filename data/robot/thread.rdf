        <dct:description><![CDATA["average":[0,0,1,2,1,2,1,2,3,3,3,0],"board":[[2,102,202,302],[1,101,201,301],[1,101,201,301],[1,101,201,301]],"worst":[[0,0,1,2,1,2,1,2,3,3,3,0]],"robots":[{"origin":[0,3,3],"code":[3,0,0],"actions":["f01","f01","f01"]},{"origin":[1,3,3],"code":[["whilenotredend","f01"],0,0]},{"origin":[2,3,3],"code":[["x3","f01"],0,0]},{"origin":[3,3,3],"code":[["f01","f01","f01"],0,0]}]]]></dct:description>
