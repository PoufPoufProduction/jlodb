    <rdf:Description>
        <dct:identifier>ad</dct:identifier>
        <dct:title xml:lang="fr-FR">Optimiser le déplacement d'un robot grâce aux cases de déplacement automatique.</dct:title>
        <dct:description><![CDATA["board":[[1,0,0],[6,6,1],[0,468,2]],"robots":[{"origin":[0,0,1],"code":[18,0,0],"actions":["f01","f01","f01","f01","f01","turnleft","turnleft","turnleft","turnleft","turnleft","turnright","turnright","turnright","turnright","turnright"]}],"padding":0]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"tips":["La case [b]automatique[/b] de déplacement ne fait pas tourner le robot.","Après les [b]2 cases automatiques[/b], l'orientation du robot n'a pas changé.","Le robot doit [b]avancer 2 fois[/b].[code][a]f01[/a][a]f01[/a][/code]"]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
