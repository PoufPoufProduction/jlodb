    <rdf:Description>
        <dct:identifier>af</dct:identifier>
        <dct:title xml:lang="fr-FR">En anticipant l'action de plusieurs cases de déplacement automatique, programmer un robot afin qu'il atteigne la case d'arrivée.</dct:title>
        <dct:description><![CDATA["board":[[1,6,7],[461,475,1],[2,8,8]],"robots":[{"origin":[0,0,0],"code":[18,0,0],"actions":["f01","f01","f01","f01","f01","b01","b01","b01","b01","b01","turnleft","turnleft","turnright","turnright","turnright"]}]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"tips":["La case [b]automatique[/b] de déplacement ne fait pas tourner le robot.","Après les [b]2 premières cases automatiques[/b], l'orientation du robot n'a pas changé. Il doit donc [b]tourner[/b].","Le programme [b]principal[/b] devrait être :[code][a]f01[/a][a]turnright[/a][a]f01[/a][/code]"]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>aa</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>ag</dct:identifier>
        <dct:title xml:lang="fr-FR">En anticipant l'action de plusieurs cases de déplacement automatique et sans pouvoir avancer, programmer un robot afin qu'il atteigne la case d'arrivée.</dct:title>
        <dct:description><![CDATA["board":[[454,7,1],[1,1,0],[2,0,0]],"robots":[{"origin":[2,0,0],"code":[18,0,0],"actions":["b01","b01","b01","b01","b01","turnleft","turnleft","turnleft","turnleft","turnleft","turnright","turnright","turnright","turnright","turnright"]}]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"tips":["La case [b]automatique[/b] de déplacement fonctionne également en marche arrière.","Le robot devoit [b]reculer 3 fois[/b] et [b]tourner 1 fois[/b].","Le programme [b]principal[/b] devrait être :[code][a]b01[/a][a]b01[/a][a]turnleft[/a][a]b01[/a][/code]"]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>2</dct:type>
