    <rdf:Description>
        <dct:identifier>ae</dct:identifier>
        <dct:title xml:lang="fr-FR">Optimiser le déplacement d'un robot grâce aux cases de déplacement automatique.</dct:title>
        <dct:description><![CDATA["board":[[1,0,0],[1,6,1],[0,468,2]],"robots":[{"origin":[0,0,1],"code":[18,0,0],"actions":["f01","f01","f01","f01","f01","turnleft","turnleft","turnleft","turnleft","turnleft","turnright","turnright","turnright","turnright","turnright"]}],"padding":0]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"tips":["Sur un parcours linéaire, la case [b]automatique[/b] de déplacement revient à une absence de case.","En tout, le robot doit [b]avancer 3 fois[/b].","Le programme [b]principal[/b] devrait être :[code][a]f01[/a][a]turnleft[/a][a]f01[/a][a]turnright[/a][a]f01[/a][/code]"]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>2</dct:type>
