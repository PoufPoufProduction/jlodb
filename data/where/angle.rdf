    <rdf:Description>
        <dct:identifier>c4</dct:identifier>
        <dct:title xml:lang="fr-FR">Représenter un angle multiple de 90.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/wallpaper01.svg","number":5,"url":"res/img/desktop/fraction01.svg","cursor":[{"cid":"cursor","init":0,"center":[320,280,190],"constraint":90,"steps":[0,1,3,6,10],"boundaries":[0,360],"onmove":"function($this,_root,_args){var d,d2; if (_args==0) { d= 'M 178,0 A 178,178 0 1 1 -178,0 A 178,178 0 1 1 178,0 z'; d2=''} else if (_args<180) { d='M0,0 v-178 a178,178 0 1,0 '+(178*Math.sin(_args*Math.PI/180))+','+(178-178*Math.cos(_args*Math.PI/180))+' z'; d2='M0,-50 a50,50 0 0,1 '+(50*Math.sin(_args*Math.PI/180))+','+(50-50*Math.cos(_args*Math.PI/180));} else { d='M0,0 v-178 a178,178 0 0,0 '+(178*Math.sin(_args*Math.PI/180))+','+(178-178*Math.cos(_args*Math.PI/180))+' z'; d2='M0,-50 a50,50 0 1,1 '+(50*Math.sin(_args*Math.PI/180))+','+(50-50*Math.cos(_args*Math.PI/180));} $(\\\"#mask\\\",_root).attr('d',d); $(\\\"#angle\\\",_root).attr('d',d2);}"}],"values":[["0","0"],["90","90"],["180","180"],["270","270"],["360","360"]],"evaluation":[5,0.5],"font":1,"svgclass":"f4a"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"comment":"[b]Réalisez[/b] l'angle demandé en tournant l'aiguille vers la [red]droite[/red]."]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>geometry</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>c8</dct:alternative>
        <dct:source>res/img/desktop/fraction01.svg,res/img/background/landscape/wallpaper01.svg</dct:source>
    </rdf:Description>


