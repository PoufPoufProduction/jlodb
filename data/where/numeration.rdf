    <rdf:Description>
        <dct:identifier>t31001</dct:identifier>
        <dct:title xml:lang="fr-FR">Créer un diagramme à partir de données numériques (3 colonnes de 0 à 10) avec légende de bas de page.</dct:title>
        <dct:description><![CDATA["url":"desktop/table01.svg","cursor":[{"id":"cursor1","init":[120,100],"constraint":[-1,30],"steps":[0,1,2,4],"boundaries":[120,100,120,400]},{"id":"cursor2","init":[200,100],"constraint":[-1,30],"steps":[0,1,2,4],"boundaries":[200,100,200,400]},{"id":"cursor3","init":[280,100],"constraint":[-1,30],"steps":[0,1,2,4],"boundaries":[280,100,280,400]}],"gen":"function() { var ret=[],l={},s=\\\"[\\\"; for (var i=0;i<3;i++) { var x=Math.floor(Math.random()*11); if (i) { s+=\\\",\\\"; } s+=x.toString(); ret.push(\\\"r\\\"+(i+1).toString()+(x<10?\\\"0\\\":\\\"\\\")+x.toString()); l[\\\"l\\\"+(i+1).toString()]=x;} s+=\\\"]\\\"; return [s,ret,l]; }","evaluation":[5,0.5],"number":10]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"comment":"Dans un repère de 0 à 10, positionner correctement les différentes colonnes"]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>numeration</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
