    <rdf:Description>
        <dct:identifier>b01</dct:identifier>
        <dct:title xml:lang="fr-FR">Atteindre l'extrémité d'un labyrinthe 10x10.</dct:title>
        <dct:description><![CDATA["sizepath":32,"colorpathf":"red","sizepoint":19,"radius":5,"gen":"function(){var svg='',nodes=[],w=40,s=[10,10],off=[20,20],maze=new jlodbmaze({x:s[0],y:s[1]}), m=maze.gen(),limits={}; svg+='<rect x=\\\"'+off[0]+'\\\" y=\\\"'+off[1]+'\\\" width=\\\"'+(maze.y*w)+'\\\" height=\\\"'+(maze.x*w)+'\\\" style=\\\"fill:white;stroke:#26A;stroke-width:2;\\\"/>';for (var i=0; i<s[0]; i++) for (var j=0; j<s[1]; j++) { var limit=[w,w,w,w]; nodes.push([(i+0.5)*w+off[0], (j+0.5)*w+off[1]]); if (!m.horiz[i][j]) {svg+='<line y1=\\\"'+(off[1]+i*w)+'\\\" y2=\\\"'+(off[1]+(i+1)*w)+'\\\" x1=\\\"'+(off[0]+(j+1)*w)+'\\\" x2=\\\"'+(off[0]+(j+1)*w)+'\\\" style=\\\"stroke:#26A;stroke-width:2;stroke-linecap:round;\\\"/>'; limit[1]=0; } if (j==0 || !m.horiz[i][j-1]) { limit[3]=0;} if (!m.verti[i][j]) {svg+='<line x1=\\\"'+(off[0]+j*w)+'\\\" x2=\\\"'+(off[0]+(j+1)*w)+'\\\" y1=\\\"'+(off[1]+(i+1)*w)+'\\\" y2=\\\"'+(off[1]+(i+1)*w)+'\\\" style=\\\"stroke:#26A;stroke-width:2;stroke-linecap:round;\\\"/>'; limit[2]=0; } if (i==0 || !m.verti[i-1][j]) { limit[0]=0;} limits[(i+j*s[0]).toString()]=limit;}  var f=6; for (var i=0;i<f;i++) for (var j=0;j<f;j++) { if ((i+j)%2==0) svg+='<rect x=\\\"'+((maze.y-1+i/f)*w+off[0])+'\\\" y=\\\"'+((maze.x-1+j/f)*w+off[1])+'\\\" width=\\\"'+(w/f)+'\\\" height=\\\"'+(w/f)+'\\\" style=\\\"fill:#26A\\\"/>'; } return {svg:svg, limits:limits, nodes:nodes, result:nodes.length-1}; }","url":"desktop/path/empty.svg","background":"res/img/background/landscape/landscape01.svg","constraint":"ortho"]]></dct:description>
