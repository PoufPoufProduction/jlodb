    <rdf:Description>
        <dct:identifier>008</dct:identifier>
        <dct:title xml:lang="fr-FR">Renseigner correctement les registres A,X et Y grâce aux instructions LDA, LDX et LDY.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["lda","ldx","ldy"],"args":[{"value":"01","type":"v"},{"value":"02","type":"v"},{"value":"A8","type":"v"},{"value":"FF","type":"v"}],"nblines":3,"valid":{"a":"ff","x":"ff","y":"ff"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Chargez les 3 registres du processeur.[/b]","[code]. 0xFF dans le registre A[br]. 0xFF dans le registre X[br]. 0xFF dans le registre Y[/code]"],"tips":["Avez-vous réellement besoin d'un indice pour [b]ce[/b] niveau ?","[b]Vraiment[/b] ?","Le code attendu est : [code]LDA #0xFF[br]LDX #0xFF[br]LDY #0xFF[/code]"]]]></dct:description>
