        <dct:identifier>007</dct:identifier>
        <dct:title xml:lang="fr-FR">Renseigner correctement les registres A,X et Y grâce aux instructions LDA, LDX et LDY.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["lda","ldx","ldy"],"args":[{"value":"01","type":"v"},{"value":"02","type":"v"},{"value":"A8","type":"v"},{"value":"FF","type":"v"}],"nblines":3,"valid":{"a":"a8","x":"01","y":"02"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Chargez les 3 registres du processeur.[/b]","[code]. 0xA8 dans le registre A[br]. 0x01 dans le registre X[br]. 0x02 dans le registre Y[/code]"],"tips":["Avez-vous réellement besoin d'un indice pour [b]ce[/b] niveau ?","[b]Vraiment[/b] ?","Le code attendu est : [code]LDA #0xA8[br]LDX #0x01[br]LDY #0x02[/code]"]]]></dct:description>
