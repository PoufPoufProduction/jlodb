    <!-- LDA, LDX, LDY (immediate) [000-020] -->

    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Charger la valeur 0x01 dans le registre A grâce à l'instruction LDA.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["lda"],"args":[{"value":"01","type":"v"}],"nblines":1,"valid":{"a":"01"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[strong]Un registre est une petite case du processeur qui peut contenir une valeur numérique entière comprise entre 0 et 255 (0x00 et 0xFF en hexadécimal), c'est à dire : un OCTET.[/strong]","","L'opérateur [red]LDA[/red] stocke une valeur dans le [b]registre A[/b]. Elle s'utilise avec la syntaxe suivante :","[code]LDA [valeur][/code]","","[b]Chargez la valeur 0x01 dans le registre A.[/b]","A la fin de l'exécution du programme, [b]le registre A doit contenir la valeur 0x01[/b]. Vérifiez cela en regardant la zone en bas à gauche de l'écran."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>059</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>002</dct:identifier>
        <dct:title xml:lang="fr-FR">Charger les valeurs 0x01 puis 0x02 dans le registre A et observer que la deuxième opération écrase la première.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["lda"],"args":[{"value":"01","type":"v"},{"value":"02","type":"v"}],"nblines":2,"valid":{"a":"02","mem":{"1000":"a901a902"}}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[strong]En chargeant une valeur dans le registre A, l'opérateur LDA écrase la valeur précédente.[/strong]","","[b]Chargez la valeur 0x01 dans le registre A, puis la valeur 0x02 dans ce même registre.[/b]","","A la fin de l'exécution, le registre A contient la valeur 0x02 : la valeur 0x01, précédemment stockée, a disparu.","","Petite remarque, si [red]LDA[/red] est utilisée sans paramètre, la compilation du programme va afficher une erreur sur la sortie standard :","[code]LDA no sngl[/code]"]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>059</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>003</dct:identifier>
        <dct:title xml:lang="fr-FR">Charger la valeur 0x00 dans le registre A à la suite d'un code précompilé.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["lda"],"args":[{"value":"00","type":"v"}],"nblines":1,"header":[{"label":"init","value":"LDA #0x05","code":"A905","rts":false}],"valid":{"a":"00"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["La première ligne du programme est un [b]code précompilé[/b] imposé par l'exercice. Dans le cas présent, la ligne 'init' change la valeur du registre A.","","[b]Ecrasez la valeur du registre A imposée par la ligne 'init' en l'initialisant à 0x00.[/b]"]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>059</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>004</dct:identifier>
        <dct:title xml:lang="fr-FR">Charger la valeur 0x02 dans le registre X grâce à l'instruction LDX.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["ldx"],"args":[{"value":"01","type":"v"},{"value":"02","type":"v"},{"value":"A8","type":"v"},{"value":"FF","type":"v"}],"nblines":1,"valid":{"x":"02"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[strong]De la même façon que LDA charge une valeur dans le registre A, l'opérateur LDX charge une valeur dans le registre X.[/strong]","","[b]Chargez la valeur 0x02 dans le registre X.[/b]","","Le nombre de registres disponibles dépend du processeur utilisé. Le processeur simulé ici est le 6502 qui possède [b]3 registres principaux[/b] : A, X et Y."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>059</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>005</dct:identifier>
        <dct:title xml:lang="fr-FR">Renseigner correctement les registres A et X grâce aux instructions LDA et LDX.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["lda","ldx"],"args":[{"value":"01","type":"v"},{"value":"02","type":"v"},{"value":"A8","type":"v"},{"value":"FF","type":"v"}],"nblines":2,"valid":{"a":"ff","x":"02"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Chargez les valeurs suivantes :[/b]","[code]. 0xFF dans le registre A[br]. 0x02 dans le registre X[/code]"]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>059</dct:alternative>
