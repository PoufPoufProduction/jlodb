    <!-- TAX [020-030] -->

    <rdf:Description>
        <dct:identifier>021</dct:identifier>
        <dct:title xml:lang="fr-FR">Transférer une valeur depuis le registre A vers le registre X grâce à l'instruction TAX.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["lda","tax"],"args":[{"value":"ff","type":"v"}],"nblines":2,"valid":{"a":"ff","x":"ff"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[strong]L'opérateur TAX copie la valeur contenue dans le registre A vers le registre X.[/strong]","","Contrairement à l'opérateur [red]LDA[/red] qui s'utilise avec une valeur en paramètre, l'instruction [red]TAX[/red] s'utilise sans argument. En cas contraire, l'erreur de compilation [blue]TAX no imm[/blue] apparaît sur la sortie standard.","","[b]Chargez la valeur 0xFF dans le registre A puis copiez cette valeur vers le registre X.[/b]"]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>059</dct:alternative>
    </rdf:Description>

