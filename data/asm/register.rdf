    <rdf:Description>
        <dct:identifier>009</dct:identifier>
        <dct:title xml:lang="fr-FR">Charger la valeur 0x00 dans les registres A,X et Y à la suite d'un code précompilé.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["lda","ldx","ldy"],"args":[{"value":"00","type":"v"}],"nblines":3,"header":[{"label":"init","value":["LDA #0x05","LDX #0xFF","LDY #0xFF"],"code":"A9FFA2FFA0FF","rts":false}],"valid":{"a":"00","x":"00","y":"00"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Initialisez tous les registres à 0x00 après que ces derniers aient été modifié par le code précompilé 'init'.[/b]"]]]></dct:description>
