        <dct:description><![CDATA["class":"large","ops":["ldx"],"args":[{"value":"00","type":"v"},{"value":"41","type":"v"},{"value":"A9","type":"v"},{"value":"FF","type":"v"}],"nblines":1,"header":[{"label":"init","value":["..."],"code":"A9A9","rts":false}],"valid":{"a":"A9","x":"A9","y":"00"}]]></dct:description>
