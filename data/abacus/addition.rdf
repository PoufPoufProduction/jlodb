        <dct:identifier>5aa</dct:identifier>
        <dct:title xml:lang="fr-FR">Additioner pas à pas deux nombres compris entre 1 et 5 sur un boulier classique [2+2,2+1,1+4].</dct:title>
        <dct:description><![CDATA["type":"classic","data":[{"value":["+","2","2"],"target":[[0,3],[0,4]],"init":2},{"value":["+","2","1"],"target":[[0,3]],"init":2},{"value":["+","1","4"],"target":[[0,2],[0,3],[0,4],[0,5]],"init":1}]]]></dct:description>
