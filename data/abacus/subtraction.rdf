    <rdf:Description>
        <dct:identifier>100ra</dct:identifier>
        <dct:title xml:lang="fr-FR">Soustraire sans retenue deux nombres compris entre 1 et 100 sur un boulier classique [85-32,71-51,98-72].</dct:title>
        <dct:description><![CDATA["type":"classic","data":[{"value":["-","85","32"],"target":[[0,4],[1,6]],"init":85},{"value":["-","71","51"],"target":[[0,1],[1,3]],"init":71},{"value":["-","98","72"],"target":[[0,7],[1,3]],"init":98}]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"fontex":0.8,"exercice":["[b]Effectuez[/b] les [b]soustractions[/b] demandées grâce au boulier.","Sans [red]retenue[/red], la soustraction s'effectue en traitant chaque colonne [b]séparément[/b].","[red]Il faut toujours commencer par les unités.[/red]"]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>subtraction</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel,CP</dct:coverage>
        <dct:alternative>s1mo</dct:alternative>
    </rdf:Description>
    
    <rdf:Description>
        <dct:identifier>100ro</dct:identifier>
        <dct:title xml:lang="fr-FR">Soustraire sans retenue deux nombres compris entre 1 et 100 sur un boulier classique.</dct:title>
        <dct:description><![CDATA["number":5,"type":"classic","gen":"function(_value){var a,b; do { a=Math.floor(Math.random()*88)+12; b=Math.floor(Math.random()*(a-1))+1; } while (a%10<b%10); return {\\\"value\\\":[\\\"-\\\",a,b],\\\"init\\\":a}; }"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"fontex":0.8,"exercice":["[b]Effectuez[/b] les [b]soustractions[/b] demandées grâce au boulier."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>subtraction</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">CP</dct:coverage>
