    <rdf:Description>
        <dct:identifier>10o</dct:identifier>
        <dct:title xml:lang="fr-FR">Soustraire deux nombres compris entre 1 et 10 sur un boulier classique.</dct:title>
        <dct:description><![CDATA["number":5,"type":"classic","gen":"function(_value){var a=Math.floor(Math.random()*8)+2,b=Math.floor(Math.random()*(a-1))+1; return {\\\"value\\\":[\\\"-\\\",a,b],\\\"init\\\":a}; }"]]></dct:description>
