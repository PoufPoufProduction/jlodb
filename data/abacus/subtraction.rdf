    <!-- CLASSIC -->

    <rdf:Description>
        <dct:identifier>10aa</dct:identifier>
        <dct:title xml:lang="fr-FR">Soustraire pas à pas deux nombres compris entre 1 et 10 sur un boulier classique [9-4,6-5,8-2].</dct:title>
        <dct:description><![CDATA["type":"classic","data":[{"value":["-","9","4"],"target":[[0,9],[0,8],[0,7],[0,6]],"init":9},{"value":["-","6","5"],"target":[[0,6],[0,5],[0,4],[0,3],[0,2]],"init":6},{"value":["-","8","2"],"target":[[0,8],[0,7]],"init":8}]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"fontex":0.8,"exercice":["[b]Effectuez[/b] les [b]soustractions[/b] demandées grâce au boulier.","Pour vous aider dans cet exercice, le [red]premier opérande[/red] est déjà placé sur le boulier et les billes à déplacer, pas à pas, sont [red]coloriées[/red]."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>subtraction</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel,CP</dct:coverage>
        <dct:alternative>s1mo</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>10a</dct:identifier>
        <dct:title xml:lang="fr-FR">Soustraire deux nombres compris entre 1 et 10 sur un boulier classique [8-3,7-5,9-7].</dct:title>
        <dct:description><![CDATA["type":"classic","data":[{"value":["-","8","3"],"target":[[0,6]],"init":8},{"value":["-","7","5"],"target":[[0,3]],"init":7},{"value":["-","9","7"],"target":[[0,3]],"init":9}]]]></dct:description>
