    <rdf:Description>
        <dct:identifier>004</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser l'opérateur REPETE pour tracer une étoile à 5 branches.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","tg","ct","mt"],"va":["V005","V200","V135","V009"],"ma":["plus"],"cl":["rep"]},"bg":[{"id":"grid100","attr":{"class":"tiny"}},{"id":"line","attr":{"x1":320,"y1":240,"x2":320,"y2":40}},{"id":"line","attr":{"x1":320,"y1":40,"x2":202.44,"y2":201.8}},{"id":"line","attr":{"x1":202.44,"y1":201.8,"x2":392.65,"y2":140}},{"id":"line","attr":{"x1":392.65,"y1":140,"x2":202.44,"y2":78.2}},{"id":"line","attr":{"x1":202.44,"y1":78.2,"x2":320,"y2":240}}],"result":{"nb":5,"values":[[320,240,320,40,0],[320,40,202.44,201.8,0],[202.44,201.8,392.65,140,0],[392.65,140,202.44,78.2,0],[202.44,78.2,320,240,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez l'étoile à 5 branches dont chaque arête mesure 200.[/b]","","[blue]L'angle entre chaque arête est [b]144[/b].[/blue]","","Pour tracer l'étoile, la tortue doit tourner [b]5 fois[/b]. Ce faisant, elle réalise [b]2 tours complets[/b] sur elle-même. Par conséquent, l'angle de chaque rotation = 2 tours / 5 = 2 * 360 / 5 = 144."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>005</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser l'opérateur REPETE pour tracer une étoile à 8 branches.</dct:title>
