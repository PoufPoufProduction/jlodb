    <rdf:Description>
        <dct:identifier>005</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser l'opérateur AVANCE avec une valeur négative pour tracer un segment unique.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","ct","mt"],"va":["V200"],"ma":["neg"]},"bg":[{"type":"grid100","attr":{"class":"tiny"}},{"type":"line","attr":{"x1":320,"y1":240,"x2":320,"y2":440}}],"result":{"nb":1,"values":[[320,240,320,440,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"En utilisant la grille 100×100 pour vous repérer, [b]tracez le segment demandé.[/b]","tips":["L'opérateur [red]RECULE[/red] n'est pas disponible ici : il vous faut trouver autre chose.","Avancer d'une [b]distance négative[/b] revient à reculer.","Le code demandé est :[code]AVANCE -200[/code]"],"help":[{"screen":-542776367,"dialog":"Il y a de l'idée, mais il vous faut avancer dans l'[b]autre direction[/b]."}]]]></dct:description>
        <dct:extent>1</dct:extent>
