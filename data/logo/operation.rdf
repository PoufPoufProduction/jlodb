    <!-- COLORS AND MODULO -->

    <rdf:Description>
        <dct:identifier>061</dct:identifier>
        <dct:title xml:lang="fr-FR">Changer la couleur du fond de l'écran à partir d'une valeur numérique.</dct:title>
        <dct:description><![CDATA["a":{"op":["fo"],"va":["V000","V001","V002","V003","V004"]},"result":{"nb":0,"bg":"red"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Dans l'onglet des valeurs, les couleurs proposées correspondent en réalité des nombres. La correspondance est la suivante:","[code]Noir = 0, Rouge = 1, Vert = 2, Bleu = 3 et Blanc = 4.[/code]","","Il n'y a [b]pas de différence à utiliser 3 à la place de 'Bleu' et réciproquement[/b]. Les couleurs ne sont proposées que par facilité d'utilisation. Cela évite d'avoir à retenir la table de correspondance.","","[b]Changez la couleur du fond en rouge.[/b]"]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
