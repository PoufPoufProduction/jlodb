    <rdf:Description>
        <dct:identifier>203</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser les formules de trigonométrie pour calculer la longueur d'un segment.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","ct","mt"],"va":["V240","V040"],"ma":["mult","cos"]},"bg":[{"type":"grid100","attr":{"class":"tiny"}},{"type":"circle","attr":{"cx":320,"cy":240,"r":240,"class":"tiny"}},{"type":"line","attr":{"x1":320,"y1":240,"x2":474.27,"y2":56.15,"class":"tiny"}},{"type":"line","attr":{"x1":474.27,"y1":56.15,"x2":320,"y2":56.15,"class":"tiny"}},{"type":"line","attr":{"x1":320,"y1":56.15,"x2":320,"y2":240}},{"type":"text","attr":{"transform":"translate(290,65)"},"text":"C"},{"type":"text","attr":{"transform":"translate(325,280)"},"text":"A"},{"type":"text","attr":{"transform":"translate(480,65)"},"text":"B"}],"result":{"nb":1,"values":[[320,56.15,320,240,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Soit le triangle ABC rectangle en C dont l'arête AB mesure 240 et l'angle en A fait 40°. Tracez le segment AC.","","Par définition de la fonction trigonométrique cosinus, on a cos(40) = côté adjacent / hypothénus = AC/AB. Par conséquent AC = AB*cos(40) = 240*cos(40)."]]]></dct:description>
        <dct:extent>2</dct:extent>
