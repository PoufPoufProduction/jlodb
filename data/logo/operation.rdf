        <dct:identifier>104</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser le théorème de Pythagore pour tracer l'hypothénuse d'un triangle rectangle.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","ct","mt"],"va":["V100","V200"],"ma":["plus","sqrt","pow2"]},"bg":[{"type":"grid100","attr":{"class":"tiny"}},{"type":"line","attr":{"x1":320,"y1":240,"x2":320,"y2":16.39}},{"type":"line","attr":{"x1":320,"y1":16.39,"x2":409.88,"y2":60.23,"class":"tiny"}},{"type":"line","attr":{"x1":409.88,"y1":60.23,"x2":322.21,"y2":239.99,"class":"tiny"}},{"type":"text","attr":{"transform":"translate(290,30)"},"text":"C"},{"type":"text","attr":{"transform":"translate(325,280)"},"text":"A"},{"type":"text","attr":{"transform":"translate(412,75)"},"text":"B"}],"result":{"nb":1,"values":[[320,240,320,16.39,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Soit ABC un triangle rectangle en B avec AB=200 et BC=100. Grâce au théorème de pythagore, calculez la longueur de l'hypothénus afin de tracer le segment AC.","","Pour rappel, le théorème de pythagore dit que AC²=AB²+BC². C'est à dire que AC = RACINE(AB²+BC²) = RACINE(200²+100²)."]]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

