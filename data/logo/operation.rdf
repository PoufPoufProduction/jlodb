    <rdf:Description>
        <dct:identifier>201</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser les formules de trigonométrie pour calculer la longueur de segments.</dct:title>
        <dct:description><![CDATA["a":{"op":["tg","av","ct","mt","xx"],"va":["V090","V200","V002","X","V045","V135"],"ma":["div","sqrt"]},"bg":[{"type":"circle","attr":{"cx":320,"cy":240,"r":200,"class":"tiny"}},{"type":"line","attr":{"x1":320,"y1":240,"x2":320,"y2":99}},{"type":"line","attr":{"x1":320,"y1":99,"x2":179,"y2":99,"class":"tiny"}},{"type":"line","attr":{"x1":179,"y1":99,"x2":320.42,"y2":240.42,"class":"tiny"}},{"type":"text","attr":{"transform":"translate(160,90)"},"text":"C"},{"type":"text","attr":{"transform":"translate(325,280)"},"text":"A"},{"type":"text","attr":{"transform":"translate(325,90)"},"text":"B"}],"result":{"nb":1,"values":[[320,240,320,99,0]]}]]></dct:description>
