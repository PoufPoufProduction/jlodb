    <!-- FORWARD,BACKWARD,PENCIL UP -->

    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Faire avancer la tortue et tracer un segment vertical.</dct:title>
        <dct:description><![CDATA["a":{"op":["av"],"va":["V200"]},"bg":[{"type":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":240}}],"result":{"nb":1,"values":[[320,240,320,40,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez le segment de longeur 200[/b].","","Depuis le menu [blue]source[/blue] en haut à gauche, faites glisser l'opérateur [red][b]AVANCE[/b][/red] vers la première ligne de la zone de programme un peu plus bas. Ensuite, cliquez sur l'onglet [b]'1'[/b] du menu [blue]source[/blue] et faites glisser la valeur [b]200[/b] de sorte à écrire [red][b]AVANCE 200[/b][/red] dans la zone de programme.","","Lancez le programme ainsi créé. Si vous êtes satisfait du résultat, [b]cliquez sur le bouton de validation pour terminer l'exercice[/b]."]]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>002</dct:identifier>
        <dct:title xml:lang="fr-FR">Tracer un segment vertical et cacher la tortue.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","ct"],"va":["V200"]},"bg":[{"type":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":240}}],"result":{"nb":1,"values":[[320,240,320,40,0]],"ct":false}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez le segment de longeur 200 et cachez la tortue.[/b]","","L'instruction [red][b]CACHE TORTUE[/b][/red] permet de faire disparaître la tortue et ainsi de pouvoir mieux visualiser les éléments situés sous cette dernière.","","Sauf cas exceptionel comme celui-ci où l'opération [red][b]CACHE TORTUE[/b][/red] est explicitement attendue, [b]l'état de la tortue n'influe pas sur la notation finale[/b]."],"tips":["Il suffit d'écrire :[code]AVANCE 200[br]CACHE TORTUE[/code]"]]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>003</dct:identifier>
        <dct:title xml:lang="fr-FR">Tracer deux segments verticaux consécutifs.</dct:title>
        <dct:description><![CDATA["a":{"op":["av"],"va":["V100"]},"bg":[{"type":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":240}}],"result":{"nb":2,"values":[[320,240,320,140,0],[320,140,320,40,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez le segment de longeur 200 en 2 fois.[/b]"],"tips":["La [b]moitié[/b] de 200 est 100.","[code]AVANCE 100[/code] permet d'avancer la tortue de 100.","Il suffit d'écrire :[code]AVANCE 100[br]AVANCE 100[/code]"]]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>004</dct:identifier>
        <dct:title xml:lang="fr-FR">Faire avancer la tortue et tracer un unique segment vertical.</dct:title>
        <dct:description><![CDATA["a":{"op":["av"],"va":["V200","V100"]},"bg":[{"type":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":240}}],"result":{"nb":1,"values":[[320,240,320,40,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez le segment de longeur 200.[/b]","","Compte tenu des valeurs proposées, il est tout à fait possible de tracer le segment de longueur 200 en 2 fois. [red]Ne procédez à ce genre de découpage que lorsque l'énoncé le demande explicitement[/red]. [b]Ce n'est pas le cas ici[/b] : tracez le segment en une seule opération."],"tips":["A moins que cela ne soit explicitement demandé, tracez [b]toujours[/b] le trait le plus [b]long[/b] possible.","[code]AVANCE 100[br]AVANCE 100[/code] n'est pas la solution attendue dans cet exercice.","[code]AVANCE 200[/code] est la solution.[br]Mais vous le saviez..."]]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>005</dct:identifier>
        <dct:title xml:lang="fr-FR">Tracer un segment vertical distant.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","lc","bc","ct","mt"],"va":["V100"]},"bg":[{"type":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":140}}],"result":{"nb":1,"values":[[320,140,320,40,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez un segment de longeur 100 situé à une distance de 100 de la tortue.[/b]","","L'instruction [red][b]LEVE CRAYON[/b][/red] permet de ne tracer aucun trait lorsque la tortue avance.","","Inversement, l'instruction [red][b]BAISSE CRAYON[/b][/red] oblige la tortue à tracer le chemin qu'elle emprunte.","","Ces [b]2 instructions[/b] n'ont aucun rapport avec [red][b]CACHE TORTUE[/b][/red] et [red][b]MONTRE TORTUE[/b][/red]."],"tips":["En tout, le programme devra appeler [b]2[/b] fois [code]AVANCE 100[/code]","La première fois, [b]sans dessiner de trait[/b] :[code]LEVE CRAYON[br]AVANCE 100[/code]","La fois suivante, [b]en dessinant le trait[/b] :[code]BAISSE CRAYON[br]AVANCE 100[/code]"]]]></dct:description>
        <dct:extent>4</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>006</dct:identifier>
        <dct:title xml:lang="fr-FR">Tracer deux segments verticaux distants.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","lc","bc","ct","mt"],"va":["V100","V050"]},"bg":[{"type":"line","attr":{"x1":320,"y1":190,"x2":320,"y2":240}},{"type":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":90}}],"result":{"nb":2,"values":[[320,240,320,190,0],[320,90,320,40,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Faites avancer la tortue de 50, de 100 puis de 50 à nouveau en ne traçant que les 2 segments de longueur 50.[/b]","","[strong]Astuce : en cliquant sur la zone de dessin, il est possible d'en obtenir un agrandissement.[/strong]"],"tips":["La tortue doit commencer par dessiner un [b]trait de 50[/b][code]AVANCE 50[/code]","Elle doit ensuite [i]sauter[/i] un espace de [b]100[/b][code]LEVE CRAYON[br]AVANCE 100[/code]","Finalement, la tortue trace à nouveau un [b]trait de 50[/b].[code]BAISSE CRAYON[br]AVANCE 50[/code]"]]]></dct:description>
        <dct:extent>4</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>007</dct:identifier>
        <dct:title xml:lang="fr-FR">Faire reculer la tortue et tracer un segment vertical.</dct:title>
        <dct:description><![CDATA["a":{"op":["re","ct","mt"],"va":["V200"]},"bg":[{"type":"line","attr":{"x1":320,"y1":440,"x2":320,"y2":240}}],"result":{"nb":1,"values":[[320,240,320,440,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez le segment de longeur 200.[/b]","","L'instruction [red][b]RECULE[/b][/red] permet de faire reculer la tortue."]]]></dct:description>
        <dct:extent>4</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>008</dct:identifier>
        <dct:title xml:lang="fr-FR">Tracer deux segments verticaux distants.</dct:title>
        <dct:description><![CDATA["a":{"op":["re","lc","bc","ct","mt"],"va":["V100","V040"]},"bg":[{"type":"line","attr":{"x1":320,"y1":280,"x2":320,"y2":240}},{"type":"line","attr":{"x1":320,"y1":420,"x2":320,"y2":380}}],"result":{"nb":2,"values":[[320,240,320,280,0],[320,380,320,420,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Faites reculer la tortue de 40, de 100 puis de 40 à nouveau en ne traçant que les 2 segments de longueur 40.[/b]"],"tips":["La tortue doit commencer par dessiner un [b]trait de 40[/b][code]RECULE 40[/code]","Elle doit ensuite [i]sauter[/i] un espace de [b]100[/b][code]LEVE CRAYON[br]RECULE 100[/code]","Finalement, la tortue trace à nouveau un [b]trait de 40[/b].[code]BAISSE CRAYON[br]RECULE 40[/code]"]]]></dct:description>
        <dct:extent>4</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>009</dct:identifier>
        <dct:title xml:lang="fr-FR">Tracer un unique segment vertical centré à l'origine.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","re","bc","lc","ct","mt"],"va":["V200","V100"]},"bg":[{"type":"line","attr":{"x1":320,"y1":340,"x2":320,"y2":140}}],"result":{"nb":1,"values":[[320,140,320,340,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez un unique segment de longeur 200 centré sur le point d'origine.[/b]"],"tips":["L'idée est de [b]reculer[/b] la tortue [b]sans dessiner de trait[/b] pour tracer ensuite le trait demandé en [b]avançant[/b].","La tortue commence par [b]reculer sans dessiner[/b]. [code]LEVE CRAYON[br]RECULE 100[/code]","Puis la tortue trace le [b]trait de 200[/b].[code]BAISSE CRAYON[br]AVANCE 200[/code]"]]]></dct:description>
        <dct:extent>4</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>010</dct:identifier>
        <dct:title xml:lang="fr-FR">Tracer deux segments verticaux distants, symétriques par rapport à l'origine.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","re","lc","bc","ct","mt"],"va":["V200","V100","V050"]},"bg":[{"type":"line","attr":{"x1":320,"y1":90,"x2":320,"y2":190}},{"type":"line","attr":{"x1":320,"y1":390,"x2":320,"y2":290}}],"result":{"nb":2,"values":[[320,190,320,90,0],[320,290,320,390,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez les 2 segments de longueur 100 situés chacun à une distance de 50 du point d'origine.[/b]"],"tips":["La tortue doit [b]reculer[/b] vers le bas du 1er trait.[code]LEVE CRAYON[br]RECULE 50[br]RECULE 100[/code]","Par [b]2 fois[/b], le programme utilisera le code suivant :[code]BAISSE CRAYON[br]AVANCE 100[/code]","Entre les 2 traits, la tortue doit avancer de [b]100[/b] sans dessiner.[code]LEVE CRAYON[br]AVANCE 100[/code]"]]]></dct:description>
        <dct:extent>4</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <!-- TURN 90 -->

    <rdf:Description>
        <dct:identifier>021</dct:identifier>
        <dct:title xml:lang="fr-FR">Faire avancer et tourner vers la droite la tortue pour tracer deux segments consécutifs.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","td"],"va":["V200","V090"]},"bg":[{"type":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":240}},{"type":"line","attr":{"x1":320,"y1":40,"x2":520,"y2":40}}],"result":{"nb":2,"values":[[320,240,320,40,0],[320,40,520,40,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Tracez les deux segments de longeur 200.[/b]","","Si l'opérateur [red][b]AVANCE[/b][/red] permet à la tortue d'avancer sur une certaine distance, l'opérateur [red][b]TOURNE DROITE[/b][/red] permet à la tortue de tourner vers la droite d'un certain angle en [b]degré[/b]. Dans cet exercice, comme l'angle entre les 2 segments est un [b]angle droit[/b], il faut utiliser l'opération [red][b]TOURNE DROITE 90[/b][/red].","","[strong]Important : un angle droit mesure 90 degrés[/strong]"],"tips":["La tortue doit [b]TOURNER À DROITE[/b] entre [b]2[/b] actions [b]AVANCER[/b].","Vous avez vraiment besoin d'un [b]autre indice[/b] ?","La figure à tracer est :[code]AVANCE 200[br]TOURNE DROITE 90[br]AVANCE 200[/code]"]]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>022</dct:identifier>
        <dct:title xml:lang="fr-FR">Faire avancer et tourner vers la gauche la tortue pour tracer deux segments consécutifs.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","tg"],"va":["V200","V090"]},"bg":[{"type":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":240}},{"type":"line","attr":{"x1":320,"y1":40,"x2":120,"y2":40}}],"result":{"nb":2,"values":[[320,240,320,40,0],[320,40,120,40,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Utilisez l'opérateur [red][b]TOURNE GAUCHE[/b][/red] et [b]tracez les deux segments de longeur 200.[/b]"],"tips":["Un [b]angle droit[/b] fait [b]90[/b] degrés.","La tortue doit [b]TOURNER À GAUCHE[/b] entre [b]2[/b] actions [b]AVANCER[/b].","La figure à tracer est :[code]AVANCE 200[br]TOURNE GAUCHE 90[br]AVANCE 200[/code]"]]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>023</dct:identifier>
        <dct:title xml:lang="fr-FR">Faire reculer et tourner la tortue pour tracer deux segments consécutifs.</dct:title>
        <dct:description><![CDATA["a":{"op":["re","td","tg"],"va":["V200","V090"]},"bg":[{"type":"line","attr":{"x1":320,"y1":240,"x2":320,"y2":440}},{"type":"line","attr":{"x1":320,"y1":440,"x2":120,"y2":440}}],"result":{"nb":2,"values":[[320,240,320,440,0],[320,440,120,440,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["En utilisant l'opérateur [red][b]RECULE[/b][/red], [b]tracez les deux segments de longeur 200.[/b]"],"tips":["Un [b]angle droit[/b] fait [b]90[/b] degrés.","La tortue doit [b]TOURNER À DROITE[/b] entre [b]2[/b] actions [b]RECULER[/b].","La figure à tracer est :[code]RECULE 200[br]TOURNE DROITE 90[br]RECULE 200[/code]"]]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>



    <!-- FORWARD,BACKWARD,TURN 90 AND PENCIL UP -->

    <rdf:Description>
        <dct:identifier>051</dct:identifier>
        <dct:title xml:lang="fr-FR">Avec les opérations élémentaires, réaliser la figure demandée au sein d'une grille 100x100.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","re","tg","td","lc","bc","ct","mt"],"va":["V200","V100","V090"]},"bg":[{"type":"grid100","attr":{"class":"tiny"}},{"type":"line","attr":{"x1":320,"y1":140,"x2":320,"y2":240}},{"type":"line","attr":{"x1":520,"y1":140,"x2":320,"y2":140}}],"result":{"nb":2,"values":[[320,140,320,240,0],[520,140,320,140,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["En utilisant la [red]grille 100x100[/red] pour vous repérer, [b]tracez la figure demandée.[/b]","","Pour rappel, [red][i]ne découpez pas les segments[/i][/red] en sous-segments."],"tips":["La tortue doit tracer [b]2[/b] segments : le 1er de longueur [b]100[/b], le 2nd de longueur [b]200[/b].","Entre ces 2 segments, la tortue doit [b]TOURNER À DROITE[/b] suivant un angle [b]droit[/b].","La figure à tracer est :[code]AVANCE 100[br]TOURNE DROITE 90[br]AVANCE 200[br][/code]"]]]></dct:description>
        <dct:extent>5</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>052</dct:identifier>
        <dct:title xml:lang="fr-FR">Avec les opérations élémentaires, réaliser la figure demandée au sein d'une grille 100x100.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","re","tg","td","lc","bc","ct","mt"],"va":["V200","V090"]},"bg":[{"type":"grid100","attr":{"class":"tiny"}},{"type":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":240}},{"type":"line","attr":{"x1":520,"y1":40,"x2":320,"y2":40}},{"type":"line","attr":{"x1":520,"y1":40,"x2":520,"y2":240}},{"type":"line","attr":{"x1":320,"y1":240,"x2":520,"y2":240}}],"result":{"nb":4,"values":[[320,40,320,240,0],[520,40,320,40,0],[520,40,520,240,0],[520,240,320,240,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["En utilisant la [red]grille 100x100[/red] pour vous repérer, [b]tracez la figure demandée.[/b]"],"tips":["Les angles d'un [b]carré[/b] ou d'un [b]rectangle[/b] sont des [b]angles droits[/b].","La tortue doit [b]TOURNER À DROITE[/b] après chacune des [b]4[/b] actions [b]AVANCER[/b].","Ainsi débute le code :[code]AVANCE 200[br]TOURNE DROITE 90[br]AVANCE 200[br][o1]TOURNE DROITE 90[/o1][br][o2]AVANCE 200[/o2][/code]"]]]></dct:description>
        <dct:extent>5</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>
    
    <rdf:Description>
        <dct:identifier>052b</dct:identifier>
        <dct:title xml:lang="fr-FR">Avec les opérations élémentaires, réaliser la figure demandée au sein d'une grille 100x100.</dct:title>
        <dct:description><![CDATA["a":{"op":["re","td","tg"],"va":["V200","V090"]},"bg":[{"type":"grid100","attr":{"class":"tiny"}},{"type":"line","attr":{"x1":320,"y1":240,"x2":320,"y2":440}},{"type":"line","attr":{"x1":320,"y1":440,"x2":520,"y2":440}},{"type":"line","attr":{"x1":520,"y1":440,"x2":520,"y2":240}},{"type":"line","attr":{"x1":520,"y1":240,"x2":320,"y2":240}}],"result":{"nb":4,"values":[[320,240,320,440,0],[320,440,520,440,0],[520,440,520,240,0],[520,240,320,240,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["En utilisant la [red]grille 100x100[/red] pour vous repérer, [b]tracez la figure demandée.[/b]"],"tips":["Les angles d'un [b]carré[/b] ou d'un [b]rectangle[/b] sont des [b]angles droits[/b].","La tortue doit [b]TOURNER À GAUCHE[/b] après chacune des [b]4[/b] actions [b]RECULER[/b].","Ainsi débute le code :[code]RECULE 200[br]TOURNE GAUCHE 90[br]RECULE 200[br][o1]TOURNE GAUCHE 90[/o1][br][o2]RECULE 200[/o2][/code]"]]]></dct:description>
        <dct:extent>5</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>
 

    <rdf:Description>
