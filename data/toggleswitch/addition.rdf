        <dct:description><![CDATA["number":5,"gg":{"good":[3,2],"wrong":[2,2],"range":[9,1],"margin":[0.5,2],"font":2},"background":"res/img/background/landscape/graycarpet01.svg","gen":"function($this,_s,_i) {var values=[4,5,2,3,1],gg=Math.floor(Math.random()*(_s.gg.good[0]-_s.gg.good[1]+1))+_s.gg.good[1],ww=Math.floor(Math.random()*(_s.gg.wrong[0]-_s.gg.wrong[1]+1))+_s.gg.wrong[1],total,data='<div style=\\\"background-color:rgba(255,255,255,0.8);border:.1em solid white;border-width:.1em 0 .1em 0;padding-left:'+_s.gg.margin[0]+'em;margin-top:'+_s.gg.margin[1]+'em;font-size:'+_s.gg.font+'em;\\\">'; var good=[],wrong=[[0,5]],cpt2=0,nbwrong=0; total=0; for (var i=0;i<gg;i++) { var e=Math.floor(Math.random()*(_s.gg.range[1]-_s.gg.range[0]+1))+_s.gg.range[0], gmax=good.length; total+=e; for (var g=0; g<gmax;g++) { if (e+good[g][0]<=total) good.push([e+good[g][0],5]); } good.push([e,0]); } var test=[]; for (var i=_s.gg.range[1];i<=_s.gg.range[0];i++) { test.push(i); } shuffle(test); do { var e=test[cpt2%test.length],isok=true; for (var w in wrong) { var val=e+wrong[w][0]; for (var g in good) { if (good[g][0]==val) { isok=false; } } } if (isok) { for (var w in wrong) { wrong.push([e+wrong[w][0],5]); } wrong.push([e,1]); nbwrong++; } cpt2++; } while(cpt2<test.length*2 && nbwrong<ww); var values = good.concat(wrong); shuffle(values); var nb=0,result=''; for (var i in values){ if (values[i][1]<2) { if (nb) { data+='<div class=\\\"left\\\">+</div>'; } nb++; data+='<div class=\\\"a left\\\">'+values[i][0]+'</div>'; result+=values[i][1].toString(); } } data+='<div class=\\\"left\\\">=</div><div>'+total+'</div></div>'; return {data:data,result:result}; }","states":{ "nb":2,"style":[{"opacity":1},{"opacity":0.1}]},"wrong":{"style":{"color":"red"}}]]></dct:description>
