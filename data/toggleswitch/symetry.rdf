        <dct:title xml:lang="fr-FR">Inverser 6 stickers pour réaliser une figure symétrique.</dct:title>
        <dct:description><![CDATA["errratio":3,"number":2,"background":"res/img/background/landscape/blueboard01.svg","template":"desktop/toggleswitch/sym01.svg","states":{ "nb":2, "class":["c0","c1"]},"wrong":{"class":"wrong"},"gg":{"nb":[6]},"gen":"function($this,settings,_i){var nb=settings.gg.nb[_i%settings.gg.nb.length],l=String.fromCharCode(96+nb),imgs=['3ad','3b2','3c8','4e6','5ff','9e0','9e2','31c','36d','40b','41b','69a','98e','349','494'],init='',result='',txt={}; for (var i=0;i<nb;i++) { result+='1'; init+=(Math.random()<0.5?'0':'1'); var im='ext/noto/stickers/emoji_u1f'+imgs[Math.floor(Math.random()*imgs.length)]+'.svg'; txt['i'+l+i]=im; txt['ii'+l+i+'a']=im; txt['ii'+l+i+'b']=im;} return { init:init,result:result,txt:txt,svgclass:'group'+l,group:'group'+l}; }"]]></dct:description>
