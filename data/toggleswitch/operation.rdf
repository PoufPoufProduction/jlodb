	<rdf:Description>
        <dct:identifier>i10</dct:identifier>
        <dct:title xml:lang="fr-FR">Trouver parmi soustractions, additions, multiplications jusqu'à 5 et divisions par 2, la seule opération dont le résultat est exact.</dct:title>
        <dct:description><![CDATA["gg":{"ad":[7,3,7,3],"su":[7,3,7,3],"mu":[4,2,10,1],"di":[9,2,1,2],"g":true},"number":5,"background":"res/img/background/landscape/bluecarpet02.svg","automatic":"one","onclick":"onlyone","states":{ "nb":2,"style":[{"background-color":"white"},{"background-color":"yellow"}]},"wrong":{"mask":"0","style":{"background-color":"red"}},"class":"center","gen":"function($this,_s,_i) { var val=[function() { var a=Math.floor(Math.random()*_s.gg.ad[0]+_s.gg.ad[1]),b=Math.floor(Math.random()*_s.gg.ad[2]+_s.gg.ad[3]); return [ a+'+'+b,a+b]; }, function() { var a = Math.floor(Math.random()*_s.gg.su[0]+_s.gg.su[1]), b = Math.floor(Math.random()*_s.gg.su[2]+_s.gg.su[3]); return [(a+b)+'-'+b,a]; }, function(_e) { var a=Math.floor(Math.random()*_s.gg.mu[0]+_s.gg.mu[1]),b = Math.floor(Math.random()*_s.gg.mu[2]+_s.gg.mu[3]); return [a+'×'+b,a*b];}, function(_e) { var a = Math.floor(Math.random()*_s.gg.di[0]+_s.gg.di[1]), b = Math.floor(Math.random()*_s.gg.di[2]+_s.gg.di[3]); return [(a*b)+'/'+b,a]; }],t=[{'t':'array2x4','n':8}],tt=t[Math.floor(Math.random()*t.length)],r='',txt=[],ii=Math.floor(Math.random()*tt.n); for (var i=0;i<tt.n;i++) { var op=Math.floor(Math.random()*4),v=val[op](),c; do { c=v[1]+Math.floor(Math.random()*6)-3; } while(c<=0 || c==v[1]); if ((_s.gg.g&&i==ii)||((!_s.gg.g) && i!=ii)) { r+=_s.gg.g?'1':'0'; c=v[1]; } else { r+=_s.gg.g?'0':'1'; } txt.push(v[0]+'='+c); } return {'template':tt.t,'result':r,'t':txt}; }","font":0.8]]></dct:description>
