    <rdf:Description>
        <dct:identifier>c10</dct:identifier>
        <dct:title xml:lang="fr-FR">Vérifier les soustractions, additions, multiplications jusqu'à 5 et divisions par 2, de valeurs comprises entre 1 et 10.</dct:title>
        <dct:description><![CDATA["gg":{"ad":[7,3,7,3],"su":[7,3,7,3],"mu":[4,2,10,1],"di":[9,2,1,2]},"background":"res/img/background/landscape/wallpaper02.svg","template":"array3x3","gen":"function($this,_s,_id){var val=[function(_e) { var a=Math.floor(Math.random()*_s.gg.ad[0]+_s.gg.ad[1]),b=Math.floor(Math.random()*_s.gg.ad[2]+_s.gg.ad[3]); return a+'+'+b+'='+(a+b+_e); }, function(_e) { var a = Math.floor(Math.random()*_s.gg.su[0]+_s.gg.su[1]), b = Math.floor(Math.random()*_s.gg.su[2]+_s.gg.su[3]); return (a+b)+'-'+b+'='+(a+_e); }, function(_e) { var a=Math.floor(Math.random()*_s.gg.mu[0]+_s.gg.mu[1]),b = Math.floor(Math.random()*_s.gg.mu[2]+_s.gg.mu[3]); return a+'×'+b+'='+(a*b+_e);}, function(_e) { var a = Math.floor(Math.random()*_s.gg.di[0]+_s.gg.di[1]), b = Math.floor(Math.random()*_s.gg.di[2]+_s.gg.di[3]); return (a*b)+'/'+b+'='+(a+_e); }],res='',t=[]; for (var i=0; i<9; i++) { var c=Math.floor(Math.random()*2),er=0,v=Math.floor(Math.random()*val.length); res+=(1-c).toString(); if (c==0) { do { er = Math.floor(Math.random()*5)-2; } while (er==0); } t.push(val[v](er)); }; return { 'result':res,'t':t}; }","states":{ "nb":2, "style":[{"background-color":"rgba(255,255,255,0.8)","color":"#333"},{"background-color":"black","color":"#222222"}]},"wrong":{"style":{"background-color":"red","color":"pink"}},"font":0.5,"number":3]]></dct:description>
