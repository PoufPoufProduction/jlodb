        <dct:description><![CDATA["number":3,"background":"res/img/background/landscape/blueboard01.svg","onclick":"onlyone","states":{ "nb":2,"style":[{"background-color":"white","border-color":"white"},{"background-color":"white","border-color":"black"}]},"wrong":{"style":{"border-color":"red"}},"gen":"function(){var num=4,r=Math.min(255,Math.floor(Math.random()*(num+1))*256/num),g=Math.min(255,Math.floor(Math.random()*(num+1))*256/num),b=Math.min(255,Math.floor(Math.random()*(num+1))*256/num), c=Math.floor(Math.random()*6),min=20000, res='',tt=[],cc=[]; for (var i=0; i<6; i++) {var rr=0,gg=0,bb=0; if (i==c) { rr=r;gg=g;bb=b; res+='1'; } else { res+='0'; var dist=0; do { rr=Math.min(255,Math.floor(Math.random()*(num+1))*256/num); gg=Math.min(255,Math.floor(Math.random()*(num+1))*256/num); bb=Math.min(255,Math.floor(Math.random()*(num+1))*256/num); dist=Math.pow(rr-r,2)+ Math.pow(gg-g,2)+ Math.pow(bb-b,2); for (var j in cc) { var dtmp = Math.pow(rr-cc[j][0],2)+ Math.pow(gg-cc[j][1],2)+ Math.pow(bb-cc[j][2],2); if (dtmp<dist) { dist=dtmp; }} } while(dist<min); } cc.push([rr,gg,bb]); tt.push('<div class=\\\"icon\\\" style=\\\"background-color:rgb('+rr+','+gg+','+bb+');\\\"></div>');} return {result:res, t:tt,legend:'[h2]('+r+','+g+','+b+')[/h2]'}}"]]></dct:description>
