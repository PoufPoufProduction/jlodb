    <rdf:Description>
        <dct:identifier>c5</dct:identifier>
        <dct:title xml:lang="fr-FR">Vérifier le résultat de soustractions dont le diminuteur et la différence sont compris entre 1 et 5.</dct:title>
        <dct:description><![CDATA["gg":5,"background":"res/img/background/landscape/bluecarpet01.svg","number":2,"template":"array2x4","gen":"function($this,_s,_id){var result=\\\"\\\",ret=[]; for (var i=0; i<9; i++) { var chance=Math.floor(Math.random()*2),a=Math.floor(Math.random()*_s.gg)+1, b=Math.floor(Math.random()*_s.gg)+1, c=(chance==0)?a+b:b+Math.floor(Math.random()*a)+1; result+=(a+b==c)?\\\"0\\\":\\\"1\\\"; ret.push(c+\\\"-\\\"+b+\\\"=\\\"+a);} return {result:result,t:ret}}","states":{ "nb":2, "style":[{"background-color":"rgba(255,255,255,0.8)","color":"gray"},{"background-color":"black","color":"#222222"}]},"wrong":{"style":{"background-color":"red","color":"pink"}},"font":0.8]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":{"value":"[b]Barrez les opérations erronées.[/b]"}]]></dct:description>
        <dct:extent>1</dct:extent>
