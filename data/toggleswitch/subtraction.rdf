    <rdf:Description>
        <dct:identifier>3</dct:identifier>
        <dct:title xml:lang="fr-FR">Identifier le nombre 3 à partir d'une soustraction de 2 nombres (compris entre 1 et 10)</dct:title>
        <dct:description><![CDATA["gg":{"r":3,"max":10,"min":1},"background":"res/img/background/landscape/grayboard01.svg","number":3,"template":"array3x3","gen":"function($this,_s,_id){var result=\\\"\\\",ret=[]; for (var i=0; i<9; i++) { var c=(Math.random()<0.5),a,b; if (c) { do { a=Math.floor(Math.random()*_s.gg.max)+_s.gg.min; b=Math.floor(Math.random()*(a-_s.gg.min))+_s.gg.min; } while(a-b==_s.gg.r); } else { do { a=Math.floor(Math.random()*(_s.gg.max-_s.gg.r))+_s.gg.r+_s.gg.min,b=a-_s.gg.r; } while(b<_s.gg.min); } result+=(a-b==_s.gg.r)?\\\"1\\\":\\\"0\\\"; ret.push(a+\\\"-\\\"+b);} return {result:result,t:ret}}","states":{ "nb":2, "style":[{"background-color":"rgba(255,255,255,0.8)","color":"gray"},{"background-color":"orange","color":"white"}]},"wrong":{"style":{"background-color":"black","color":"red"}}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":{"value":"Surlignez en <span style='color:orange;'><b>ORANGE</b></span>, les soustractions dont le résultat vaut <b>3</b>.","tag":"3"}]]></dct:description>
        <dct:extent>3</dct:extent>
