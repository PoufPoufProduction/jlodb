	<rdf:Description>
        <dct:identifier>d01</dct:identifier>
        <dct:title xml:lang="fr-FR">Surligner les heures comprises dans un intervalle de temps.</dct:title>
        <dct:description><![CDATA["errratio":3,"number":2,"gg":[0,12],"class":"bigl","background":"res/img/background/landscape/grayboard01.svg","gen":"function($this,_s,_id){ var t=[],r='',d=Math.floor(Math.random()*_s.gg[0])+_s.gg[1],min=Math.floor(Math.random()*(144-d)),max=min+d; for (var i=0; i<9; i++) { var c; if (Math.random()<0.5) { do { c=Math.floor(Math.random()*144);} while(c<=min || c>=max); r+='1'; } else { do { c=Math.floor(Math.random()*144);} while(c>=min && c<=max); r+='0'; } t.push('[icon]res/img/clock/clockA'+(c<120?'0':'')+Math.floor(c/12)+((c%12)<2?'0':'')+((c%12)*5)+'.svg[/icon]'); } return {'template':'array3x3r','result':r,'t':t,'legend':'[h1]'+Math.floor(min/12)+':'+((min%12)<2?'0':'')+((min%12)*5)+'[/h1][h1]↓[/h1][h1]'+Math.floor(max/12)+':'+((max%12)<2?'0':'')+((max%12)*5)+'[/h1]'} }","states":{ "nb":2, "style":[{"background-color":"rgba(255,255,255,0.8)","color":"gray"},{"background-color":"red","color":"white"}]},"wrong":{"style":{"background-color":"black","color":"red"}}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":{"value":"[b]Surlignez[/b] en <span style='color:red;'><b>ROUGE</b></span>, les heures comprises dans l'intervalle."}]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>time</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>d01</dct:alternative>
