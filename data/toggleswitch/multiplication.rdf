        <dct:description><![CDATA["gg":[1,2,10,1],"number":2,"background":"res/img/background/landscape/grayboard01.svg","template":"array2x4","gen":"function($this,_s,_i){var result='',ret=[]; for (var i=0; i<9; i++) { var chance=(Math.random()<0.5), a=Math.floor(Math.random()*_s.gg[0])+_s.gg[1],b=Math.floor(Math.random()*_s.gg[2])+_s.gg[3], c=a*b; if (chance) { do {c=Math.floor(Math.random()*_s.gg[0]*_s.gg[2])+_s.gg[1]*_s.gg[3]; } while (c==a*b); } result+=(a*b==c)?'0':'1'; ret.push(a+'×'+b+'='+c);} return {result:result,t:ret}}","states":{ "nb":2, "style":[{"background-color":"rgba(255,255,255,0.8)","color":"gray"},{"background-color":"black","color":"#222222"}]},"wrong":{"style":{"background-color":"red","color":"pink"}}]]></dct:description>
