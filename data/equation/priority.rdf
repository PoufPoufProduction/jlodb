    <!-- OPERATIONS PRIORITY [000-100] -->

    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Réaliser une opération complexe [3+2*2].</dct:title>
        <dct:description><![CDATA["figure":"background/equation/default.svg","font":0.7,"data":[{"value":"3+2*2"}],"source":[],"result":["7"],"top":168, "a":{"all":false,"move":true}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["La multiplication est prioritaire par rapport à l'addition. C'est à dire qu'il faut réaliser l'opération en commençant par la multiplication pour calculer ensuite l'addition."]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>operations</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>105</dct:alternative>
    </rdf:Description>

