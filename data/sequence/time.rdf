    <rdf:Description>
        <dct:identifier>c03</dct:identifier>
        <dct:title xml:lang="fr-FR">Lire, sans contrainte de temps, les heures ou les minutes depuis une horloge à cadran.</dct:title>
        <dct:description><![CDATA["time":0,"number":15,"regexp":{"output":"\\\\?\\\\?","input": { "from": "(.+) (.+)","to":"<div class='icon' style='font-size:3.6em;float:none;margin:.1em auto;'><img src='res/img/clock/clockA$1.svg' alt=''/></div><div style='font-size:1em;'>$2</div>"}},"input":{"digit":2,"speed":5000},"gen":"function(){var h=Math.floor(Math.random()*12),m=Math.floor(Math.random()*12)*5,hh=h,mm=m; if (hh<10) { hh='0'+hh;} if (mm<10) { mm='0'+mm; } return Math.random()>0.5?([ hh.toString()+mm+' ??:'+mm,[hh,h+12,h]]):([ hh.toString()+mm+' '+hh+':??',[mm,m]]);}","class":"neg alpha h60","font":0.8]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>time</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
