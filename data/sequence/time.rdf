<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:dct="http://purl.org/dc/terms/">

    <rdf:Description>
        <dct:identifier>c01</dct:identifier>
        <dct:title xml:lang="fr-FR">Lire, sans contrainte de temps, les minutes depuis une horloge à cadran.</dct:title>
        <dct:description><![CDATA["time":0,"number":15,"regexp":{"output":"\\\\?\\\\?","input": { "from": "(.+) (.+)","to":"<div class='icon' style='font-size:3.6em;float:none;margin:.1em auto;'><img src='res/img/clock/clockA$1.svg' alt=''/></div><div style='font-size:1em;'>$2</div>"}},"input":{"digit":2,"speed":5000},"gen":"function(){var h=Math.floor(Math.random()*12),m=Math.floor(Math.random()*12)*5,hh=h,mm=m; if (hh<10) { hh='0'+hh;} if (mm<10) { mm='0'+mm; } return [ hh.toString()+mm+' '+hh+':??',[mm,m]];}","class":"neg alpha h60","font":0.8]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>time</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>c01</dct:alternative>
        <dct:source>res/img/clock/clockA*.svg</dct:source>
    </rdf:Description>
    
    <rdf:Description>
        <dct:identifier>c02</dct:identifier>
        <dct:title xml:lang="fr-FR">Lire, sans contrainte de temps, les heures depuis une horloge à cadran.</dct:title>
        <dct:description><![CDATA["time":0,"number":15,"regexp":{"output":"\\\\?\\\\?","input": { "from": "(.+) (.+)","to":"<div class='icon' style='font-size:3.6em;float:none;margin:.1em auto;'><img src='res/img/clock/clockA$1.svg' alt=''/></div><div style='font-size:1em;'>$2</div>"}},"input":{"digit":2,"speed":5000},"gen":"function(){var h=Math.floor(Math.random()*12),m=Math.floor(Math.random()*12)*5,hh=h,mm=m; if (hh<10) { hh='0'+hh;} if (mm<10) { mm='0'+mm; } return [ hh.toString()+mm+' ??:'+mm,[hh,h+12,h]];}","class":"neg alpha h60","font":0.8]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>time</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>c01</dct:alternative>
        <dct:source>res/img/clock/clockA*.svg</dct:source>
