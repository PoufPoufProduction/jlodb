    <rdf:Description>
        <dct:identifier>c01</dct:identifier>
        <dct:title xml:lang="fr-FR">Lire, sans contrainte de temps, les minutes depuis une horloge à cadran.</dct:title>
        <dct:description><![CDATA["gg":[1,12,12,1],"time":0,"number":15,"input":{"digit":2,"speed":5000},"gen":"function($this, _s, _id){var h=Math.floor(Math.random()*_s.gg[1]),m=Math.floor(Math.random()*_s.gg[2])*5,hh=h,mm=m,ret,leg; if (hh<10) { hh='0'+hh;} if (mm<10) { mm='0'+mm; } if (Math.random()>_s.gg[3]) { leg = '??:'+mm; ret = [hh,h+12,h]; } else { leg = hh+':??'; ret = [mm,m]; } return [ '<div class=\\\"icon\\\" style=\\\"font-size:3.6em;float:none;margin:.1em auto;background-image:url(res/img/clock/swatch0'+_s.gg[0]+'.svg)\\\"><img src=\\\"res/img/clock/clock'+(hh.toString()+mm)+'.svg\\\" alt=\\\"\\\"/></div><div style=\\\"font-size:1em;\\\">'+leg+'</div>',ret];}","class":"neg alpha h60","font":0.8]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>time</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>c01</dct:alternative>
        <dct:source>res/img/clock/*.svg</dct:source>
    </rdf:Description>
    
