	<!-- DECIMAL -->
	
	<rdf:Description>
        <dct:identifier>dd1</dct:identifier>
        <dct:title xml:lang="fr-FR">Diviser, sans contrainte de temps, par une puissance de 10</dct:title>
        <dct:description><![CDATA["time":0,"decimal":true,"input":{"digit":5,"speed":5000},"gg":{"d":[10,100,1000],"dd":[1],"v":[100,1000,10000],"max":100,"min":1000},"gen":"function($this,_s,_i) { var a,b; do { a=pick(_s.gg.d); b=(Math.floor(Math.random()*pick(_s.gg.v))+1)/pick(_s.gg.dd);} while (b/a>_s.gg.max || ((b/a)*_s.gg.min)%1!=0); return [jtools.num.tostr(b)+jlo.div+jtools.num.tostr(a),b/a]; }","font":1,"class":"h12 p02","number":15]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>dd1</dct:alternative>
        <dct:coverage xml:lang="fr-FR">décimal</dct:coverage>
    </rdf:Description>
	
