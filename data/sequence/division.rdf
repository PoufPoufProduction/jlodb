    <rdf:Description>
        <dct:identifier>0100</dct:identifier>
        <dct:title xml:lang="fr-FR">Résoudre, sans contrainte de temps, la division d'un nombre compris entre 1 et 100.</dct:title>
        <dct:description><![CDATA["time":0,"input":{"digit":2,"speed":5000},"gen":"function(){var a=Math.floor(Math.random()*10)+1, b=Math.floor(Math.random()*10)+1; return [ jtools.num.tostr(a*b)+jlo.div+jtools.num.tostr(a),b]; }","font":2.3,"number":15,"class":"p02"]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>01k</dct:alternative>
    </rdf:Description>

