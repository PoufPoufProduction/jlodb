        <dct:description><![CDATA["time":0,"input":{"digit":2,"speed":5000},"regexp": { "output": "\\\\?" },"gen":"function(){var a=Math.floor(Math.random()*10)+1, b=Math.floor(Math.random()*9)+2, c = Math.floor(Math.random()*(b-1))+1; return [ jtools.num.tostr(a*b+c)+' = ?'+jlo.mult+b+'+'+c, a]; }","font":1,"number":10,"class":"p02"]]></dct:description>
