    <rdf:Description>
        <dct:identifier>004</dct:identifier>
        <dct:title xml:lang="fr-FR">Identifier, sans contrainte de temps, un animal suivant sa position [haut, bas, gauche, droite].</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/grayboard01.svg","number":15,"keyboard":false,"input":{"digit":1,"svg":"res/img/desktop/sequence/grid7x4.svg","key":".key","gen":"function($this,_s,_i){var t={}, idx=0; _s.values=[]; shuffle(_s.gg.d); for (var i=0; i<_s.gg.s[0]; i++)for (var j=0; j<_s.gg.s[1]; j++){ t['i'+i+''+j]=_s.gg.p+_s.gg.d[idx][0]+'.svg'; for (var tt in _s.gg.t) {var ct=_s.gg.t[tt],ii=i+ct[0],jj=j+ct[1]; if (ii>=0&&ii<_s.gg.s[0]&&jj>=0&&jj<_s.gg.s[1]) { _s.values.push([ct[2].replace('%s',_s.gg.d[idx][1]),'e'+ii+''+jj]); } } idx++;  } return {'t':t}}"},"class":"alpha neg h12", "font":0.6]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"gg":{"s":[7,4],"t":[[0,-1,"Je suis [red]au-dessus[/red] %s."],[1,0,"Je suis [red]à droite[/red] %s."],[0,1,"Je suis [red]au-dessous[/red] %s."],[-1,0,"Je suis [red]à gauche[/red] %s."]],"p":"res/img/clipart/animal/","d":[["dog04","du [b]chien[/b]"],["cow03","de la [b]vache[/b]"],["crab01","du [b]crabe[/b]"],["chicken01","de la [b]poule[/b]"],["eagle02","de l'[b]aigle[/b]"],["snake01","du [b]serpent[/b]"],["rhinoceros01","du [b]rhinocéros[/b]"],["camel01","du [b]dromadaire[/b]"],["bison01","du [b]bison[/b]"],["giraffe02","de la [b]girafe[/b]"],["frog01","de la [b]grenouille[/b]"],["mice01","de la [b]souris[/b]"],["crow01","du [b]corbeau[/b]"],["rabbit03","du [b]lapin[/b]"],["honey01","de l'[b]abeille[/b]"],["crocodile01","du [b]crocodile[/b]"],["panda02","du [b]panda[/b]"],["elephant01","de l'[b]éléphant[/b]"],["turtle01","de la [b]tortue[/b]"],["pig01","du [b]cochon[/b]"],["octopus01","de la [b]pieuvre[/b]"],["fish03","du [b]poisson[/b]"],["donkey01","de l'[b]âne[/b]"],["zebra01","du [b]zèbre[/b]"],["snail01","de l'[b]escargot[/b]"],["goose01","de l'[b]oie[/b]"],["whale02","de la [b]baleine[/b]"],["cat01","du [b]chat[/b]"]]},"comment":"Qui suis-je ?"]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>position</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>001</dct:alternative>
        <dct:source>res/img/background/landscape/grayboard01.svg,res/img/desktop/sequence/grid7x4.svg,res/img/clipart/animal/dog04.svg,res/img/clipart/animal/cow03.svg,res/img/clipart/animal/crab01.svg,res/img/clipart/animal/chicken01.svg,res/img/clipart/animal/eagle02.svg,res/img/clipart/animal/snake01.svg,res/img/clipart/animal/rhinoceros01.svg,res/img/clipart/animal/camel01.svg,res/img/clipart/animal/bison01.svg,res/img/clipart/animal/giraffe02.svg,res/img/clipart/animal/frog01.svg,res/img/clipart/animal/mice01.svg,res/img/clipart/animal/crow01.svg,res/img/clipart/animal/rabbit03.svg,res/img/clipart/animal/honey01.svg,res/img/clipart/animal/crocodile01.svg,res/img/clipart/animal/panda02.svg,res/img/clipart/animal/elephant01.svg,res/img/clipart/animal/turtle01.svg,res/img/clipart/animal/pig01.svg,res/img/clipart/animal/octopus01.svg,res/img/clipart/animal/fish03.svg,res/img/clipart/animal/donkey01.svg,res/img/clipart/animal/zebra01.svg,res/img/clipart/animal/snail01.svg,res/img/clipart/animal/goose01.svg,res/img/clipart/animal/whale02.svg,res/img/clipart/animal/cat01.svg</dct:source>
