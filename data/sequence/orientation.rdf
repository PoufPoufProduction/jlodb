    <rdf:Description>
        <dct:identifier>004</dct:identifier>
        <dct:title xml:lang="fr-FR">Identifier, sans contrainte de temps, un animal suivant sa position [haut, bas, gauche, droite].</dct:title>
        <dct:description><![CDATA["number":15,"time":0,"template":"svg.html","keyboard":false,"input":{"digit":1,"svg":"res/img/desktop/animals01.svg","key":".key"},"class":"alpha neg h12", "font":0.6]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"comment":"Qui suis-je ?","values":[["Je suis [b]au-dessus[/b] du [red]dromadaire[/red]","dog"],["Je suis [b]au-dessus[/b] du [red]panda[/red]","frog"],["Je suis [b]au-dessus[/b] du [red]corbeau[/red]","snake"],["Je suis [b]au-dessus[/b] de la [red]tortue[/red]","crow"],["Je suis [b]au-dessus[/b] du [red]poisson[/red]","crocodile"],["Je suis à [b]gauche[/b] de l'[red]éléphant[/red]","panda"],["Je suis à [b]gauche[/b] du [red]poisson[/red]","octopus"],["Je suis à [b]gauche[/b] de l'[red]escargot[/red]","zebra"],["Je suis à [b]gauche[/b] de la [red]grenouille[/red]","girafe"],["Je suis à [b]gauche[/b] du [red]panda[/red]","cat"],["Je suis [b]au-dessous[/b] du [red]crabe[/red]","girafe"],["Je suis [b]au-dessous[/b] du [red]cochon[/red]","whale"],["Je suis [b]au-dessous[/b] de l'[red]aigle[/red]","mice"],["Je suis [b]au-dessous[/b] de l'[red]abeille[/red]","octopus"],["Je suis [b]au-dessous[/b] de la [red]poule[/red]","frog"],["Je suis à [b]droite[/b] du [red]serpent[/red]","rhinoceros"],["Je suis à [b]droite[/b] du [red]bison[/red]","girafe"],["Je suis à [b]droite[/b] de la [red]vache[/red]","crab"],["Je suis à [b]droite[/b] du [red]crocodile[/red]","cat"],["Je suis à [b]droite[/b] de la [red]grenouille[/red]","mice"]]]]></dct:description>
        <dct:extent>2</dct:extent>
