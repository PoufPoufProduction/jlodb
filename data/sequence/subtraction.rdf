    <rdf:Description>
        <dct:identifier>10</dct:identifier>
        <dct:title xml:lang="fr-FR">Effectuer, sans contrainte de temps, des soustractions dont la diminuende est comprise entre 5 et 10, et la différence positive.</dct:title>
        <dct:description><![CDATA["gg":{"dim":[5,6]},"class":"h12","time":0,"input":{"digit":2,"speed":5000},"gen":"function($this,_s,_id) { var ret=''; if (_s.gg.dim) { var a=Math.floor(Math.random()*_s.gg.dim[0])+_s.gg.dim[1]; ret = a+'-'+(Math.floor(Math.random()*a)+1); } else { var a=Math.floor(Math.random()*_s.gg.val[0])+_s.gg.val[1], b=Math.floor(Math.random()*_s.gg.val[2])+_s.gg.val[3]; ret=(a+b)+'-'+b; } return ret; }","font":2.4]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>subtraction</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>100</dct:alternative>
        <dct:coverage xml:lang="fr-FR">CP</dct:coverage>
    </rdf:Description>
	
