    <rdf:Description>
        <dct:identifier>d2</dct:identifier>
        <dct:title xml:lang="fr-FR">Calculer, sans contrainte de temps, le double d'un nombre compris entre 1 et 10.</dct:title>
        <dct:description><![CDATA["number":10,"class":"h12","time":0,"input":{"digit":2,"speed":5000},"regexp": { "output": "\\\\?" },"values":[["1+1 = 1×2 = ?",2],["2+2 = 2×2 = ?",4],["3+3 = 3×2 = ?",6],["4+4 = 4×2 = ?",8],["5+5 = 5×2 = ?",10],["6+6 = 6×2 = ?",12],["7+7 = 7×2 = ?",14],["8+8 = 8×2 = ?",16],["9+9 = 9×2 = ?",18],["10+10 = 10×2 = ?",20]],"font":0.8]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>multiplication</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>d2</dct:alternative>
        <dct:coverage xml:lang="fr-FR">CP</dct:coverage>
    </rdf:Description>
    
    
