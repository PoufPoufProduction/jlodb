        <dct:description><![CDATA["gg":{"m":[10000,1],"n":[0,1],"d":[10,100,1000],"o":[10,100,1000],"r":[1]},"number":15,"class":"h12 p02","time":0,"template":"keypad","decimal":true,"input":{"digit":5,"speed":5000},"gen":"function($this,_s,_i) { var m,n,d,e,o,r; do { m=Math.floor(Math.random()*_s.gg.m[0])+_s.gg.m[1]; d=pick(_s.gg.d); n=Math.floor(Math.random()*_s.gg.n[0])+_s.gg.n[1]; e=(n==1)?1:pick(_s.gg.d); o=pick(_s.gg.o); r=pick(_s.gg.r); } while ((o*r*(m/d)*(n/e)>100000)||((o*r*(m/d)*(n/e)*10000)%10!=0)); var ops=[jtools.num.tostr(m/d),jtools.num.tostr(o)]; if (r!=1) { ops.push(jtools.num.tostr(r)); } if (n!=1) { ops.push(jtools.num.tostr(n/e)); } if (r!=1 || n!=1) { shuffle(ops); } return [ops.join(jlo.mult),Math.round(10000*o*r*(m/d)*(n/e))/10000]; }","font":1]]></dct:description>
