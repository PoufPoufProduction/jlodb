<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:dct="http://purl.org/dc/terms/">


    <rdf:Description>
        <dct:identifier>m01</dct:identifier>
        <dct:title xml:lang="fr-FR">Trouver, sans contrainte de temps, la lettre manquante selon l'ordre alphabétique.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/c_yellow04.svg","number":15,"input":{"id":"azertyup","digit":1,"speed":5000},"regexp":{"output":"\\\\?"},"gen":"function(){ var val=Math.floor(Math.random()*26), b=val-2,ret=\\\"\\\",pre=\\\"...\\\",suf=\\\"...\\\"; if (b<0) { b = 0; pre=\\\"\\\";} if (b+5>25) { b=20; suf=\\\"\\\"; } for (var i=b;i<b+6;i++) { ret+=(i==val?\\\"<span style='color:red;'>?</span>\\\":String.fromCharCode(65+i)); } return [pre+ret+suf,String.fromCharCode(65+val)];}","class":"neg alpha","font":1.5]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"comment":"Selon l'ordre [b]alphabétique[/b], quelle est la [red]lettre manquante[/red] ?"]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>languages</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>m01</dct:alternative>
        <dct:source>res/img/background/landscape/c_yellow04.svg</dct:source>
    </rdf:Description>
        
    <rdf:Description>
        <dct:identifier>ma01</dct:identifier>
        <dct:title xml:lang="fr-FR">Trouver, sans contrainte de temps, la lettre suivante selon l'ordre alphabétique.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/c_yellow04.svg","number":15,"input":{"id":"azertyup","digit":1,"speed":5000},"gen":"function(){ var val=Math.floor(Math.random()*25); return [String.fromCharCode(65+val),String.fromCharCode(65+val+1)];}","class":"neg alpha","font":2]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"comment":"Selon l'ordre [b]alphabétique[/b], quelle est la [red]lettre suivante[/red] ?"]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>languages</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>m01</dct:alternative>
        <dct:source>res/img/background/landscape/c_yellow04.svg</dct:source>
    </rdf:Description>
        
    <rdf:Description>
        <dct:identifier>mb01</dct:identifier>
        <dct:title xml:lang="fr-FR">Trouver, sans contrainte de temps, la lettre précédente selon l'ordre alphabétique.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/c_yellow04.svg","number":15,"input":{"id":"azertyup","digit":1,"speed":5000},"gen":"function(){ var val=Math.floor(Math.random()*25); return [String.fromCharCode(65+val+1),String.fromCharCode(65+val)];}","class":"neg alpha","font":2]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"comment":"Selon l'ordre [b]alphabétique[/b], quelle est la [red]lettre précédente[/red] ?"]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>languages</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>m01</dct:alternative>
        <dct:source>res/img/background/landscape/c_yellow04.svg</dct:source>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>n001</dct:identifier>
        <dct:title xml:lang="fr-FR">Indiquer, sans contrainte de temps, la position de 2 mots suivant l'ordre alphabétique.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/book01.svg","number":15,"regexp":{"output":"\\\\.\\\\.\\\\.","input": { "from": "(.+) (.+) (.+)","to":"<div style='float:left;width:5em;text-align:center;'>$1</div><div style='float:left;width:4em;font-size:0.6em;text-align:center;margin-top:.5em;'>$2</div><div style='float:left;width:5em;text-align:center;'>$3</div>"}}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"input":{"attr":[4,3.8,1.2],"digit":1,"speed":0,"values":["avant","après"]},"class":"neg alpha h12 p02 vertical","font":0.8,"gen":"function(){var w=['Abricot','Aigle','Banane','Brebis','Cerise','Chien','Daim','Date','Éléphant','Endive','Fraise','Furet','Gorille','Groseille','Haricot','Hérisson','Ibis','Jument','Kiwi','Koala','Laitue','Lion','Mangue','Mouton','Noix','Oignon','Otarie','Panda','Pomme','Raisin','Renard','Salade','Serpent','Tomate','Tortue','Vache','Zèbre'],a=Math.floor(Math.random()*w.length),b; do { b=Math.floor(Math.random()*w.length); } while (a==b); return [w[a]+' ... '+w[b],a<b?'avant':'après'];}","comment":"Selon l'ordre [b]alphabétique[/b], quelle est la [red]position[/red] du premier mot par rapport au second ?","fontex":0.8]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>languages</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>n001</dct:alternative>
        <dct:source>res/img/background/book01.svg</dct:source>
    </rdf:Description>
    

</rdf:RDF>


