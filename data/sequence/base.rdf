        <dct:description><![CDATA["time":0,"template":"keypad8.html","input":{"digit":2,"speed":2000},"regexp":{"input": { "from": "([^ ]+)","to":"<div class=\\\"icon\\\"><img src=\\\"res/img/numbers/box$1.svg\\\"/></div>"} },"gen":"function(){var a=Math.floor(Math.random()*16)+1, b=a, ret=\\\"\\\"; while(a>0) { if (ret.length) { ret+=\\\" \\\"; } if (a>=64) { ret+=\\\"64\\\"; a-=64; } else if (a>=32) { ret+=\\\"32\\\"; a-=32; } else if (a>=24) { ret+=\\\"24\\\"; a-=24; } else if (a>=16) { ret+=\\\"16\\\"; a-=16; } else if (a>=8) { ret+=\\\"08b\\\"; a-=8; } else { ret+=\\\"0\\\"+a; a=0; } } return [ret, b.toString(8)];}","class":"wrong gray","font":3.4]]></dct:description>
