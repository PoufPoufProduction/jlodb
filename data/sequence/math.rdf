        <dct:description><![CDATA["number":10,"time":0,"input":{"digit":3,"speed":5000},"gen":"function() {var ret='<div class=\\\"icon\\\" style=\\\"margin-left:0.5em;\\\"><svg xmlns=\\\"http://www.w3.org/2000/svg\\\" width=\\\"100%\\\" height=\\\"100%\\\" viewBox=\\\"0 0 96 96\\\"><defs><style>rect {fill:white;fill-opacity:0.5;stroke:black}path{fill:none;stroke:black}text{font-size:20px;text-anchor:middle;font-family:Times New Roman;}</style></defs><rect x=\\\"38\\\" y=\\\"1\\\" width=\\\"20\\\" height=\\\"20\\\"/><path d=\\\"M 48,21 48,24 24,24 24,27 M 48,24 72,24 72,27\\\"/><rect x=\\\"14\\\" y=\\\"27\\\" width=\\\"20\\\" height=\\\"20\\\"/><rect x=\\\"62\\\" y=\\\"27\\\" width=\\\"20\\\" height=\\\"20\\\"/>',wrong=0,op=[['+',1,10,function(a,b){return a+b}],['-',1,20,function(a,b){return a>b?a-b:wrong}],['×',1,10,function(a,b){return a>10||b>10?wrong:a*b}],['/',2,20,function(a,b){return a%b==0&&a!=b&&b!=1?a/b:wrong}]],root=op[Math.floor(Math.random()*op.length)],rr,ret2,child=[-1,-1],val=[[0,0,0],[0,0,0]]; do { ret2=''; for (var i=0; i<2; i++) { child[i]=Math.random()<0.4?-1:Math.floor(Math.random()*op.length); if (child[i]==-1) {val[i][0]=Math.floor(Math.random()*10)+1; ret2+='<text transform=\\\"translate('+(24+i*48)+',44)\\\">'+val[i][0]+'</text>'} else { var node=op[Math.floor(Math.random()*op.length)]; ret2+='<g transform=\\\"translate('+(i*48)+',0)\\\"><path d=\\\"M 24,47 24,50 12,50 12,53 M 24,50 36,50 36,53\\\"/><rect x=\\\"2\\\" y=\\\"53\\\" width=\\\"20\\\" height=\\\"20\\\"/><rect x=\\\"26\\\" y=\\\"53\\\" width=\\\"20\\\" height=\\\"20\\\"/></g>'; do { val[i][1]=Math.floor(Math.random()*10)+1; val[i][2]=Math.floor(Math.random()*10)+1; val[i][0]=node[3](val[i][1],val[i][2]); } while(val[i][0]==wrong); ret2+='<text transform=\\\"translate('+(12+i*48)+',70)\\\">'+val[i][1]+'</text><text transform=\\\"translate('+(36+i*48)+',70)\\\">'+val[i][2]+'</text><text transform=\\\"translate('+(24+i*48)+',44)\\\">'+node[0]+'</text>'; }} rr=root[3](val[0][0],val[1][0]); } while(rr==wrong); ret+=ret2+'<text transform=\\\"translate(48,18)\\\">'+root[0]+'</text></svg></div>'; return [ret,rr];}","class":"alpha p02 neg", "font":4]]></dct:description>
