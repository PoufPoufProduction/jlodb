        <dct:title xml:lang="fr-FR">Résoudre une opération présentée sous la forme d'un arbre [addition,soustraction,multiplication par 1 ou 2].</dct:title>
        <dct:description><![CDATA["time":0,"input":{"digit":2,"speed":5000},"gen":"function() {var ret='<div class=\\\"icon\\\" style=\\\"margin-left:0.5em;\\\"><svg width=\\\"100%\\\" height=\\\"100%\\\" viewBox=\\\"0 0 48 48\\\"><defs><style>rect {fill:white;fill-opacity:0.5;stroke:black;}path{fill:none;stroke:black;}text{font-size:20px;text-anchor:middle;font-family:Times New Roman;}</style></defs><rect x=\\\"14\\\" y=\\\"1\\\" width=\\\"20\\\" height=\\\"20\\\"/><path d=\\\"M 24,21 24,24 11,24 11,27 M 24,24 37,24 37,27\\\"/><rect x=\\\"1\\\" y=\\\"27\\\" width=\\\"20\\\" height=\\\"20\\\"/><rect x=\\\"27\\\" y=\\\"27\\\" width=\\\"20\\\" height=\\\"20\\\"/>',wrong=0,op=[['+',1,10,function(a,b){return a+b}],['-',1,10,function(a,b){return a>b?a-b:wrong}],['*',1,10,function(a,b){return b<3?a*b:wrong}],['/',2,20,function(a,b){return a%b==0&&a!=b?a/b:wrong}]],aa,bb,rr,oo=Math.floor(Math.random()*3); do { aa=Math.floor(op[oo][1]+Math.random()*op[oo][2]);bb=Math.floor(op[oo][1]+Math.random()*op[oo][2]);rr=op[oo][3](aa,bb); }while(rr==wrong); ret+='<text transform=\\\"translate(24,18)\\\">'+op[oo][0]+'</text><text transform=\\\"translate(11,43)\\\">'+aa+'</text><text transform=\\\"translate(37,43)\\\">'+bb+'</text></svg></div>'; return [ret,rr];}","class":"alpha p02 neg", "font":4]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>mathematics</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>tr01</dct:alternative>
        <dct:coverage xml:lang="fr-FR">CP</dct:coverage>
    </rdf:Description>

