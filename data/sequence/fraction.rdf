        <dct:description><![CDATA["gg":[[2],[4]],"number":10,"time":0,"input":{"attr":[1.8,1.2,1.2],"digit":1,"values":["&lt;","=","&gt;"]},"regexp": { "output": "\\\\?" },"template":"template2.html","gen":"function($this,_s,_i){ var de1=_s.gg[0][Math.floor(Math.random()*_s.gg[0].length)], de2=_s.gg[1][Math.floor(Math.random()*_s.gg[1].length)], num1=Math.floor(Math.random()*de1+1), num2=Math.floor(Math.random()*de2+1), op=num1+' '+de1+' / .mar '+num2+' '+de2+' / .mar .nop',tree = jtools.math.pol2tree(op), svg = jtools.math.tree2svg(tree, {}); var ratio = (5*svg.size[0])/(10*svg.size[1]); return [\\\"<div style='width:\\\"+Math.min(100,ratio*100)+\\\"%;margin:0 auto;'>\\\"+svg.svg+'</div>', (num1/de1==num2/de2)?\\\"=\\\":((num1/de1<num2/de2)?\\\"&lt;\\\":\\\"&gt;\\\")];}","class":"alpha p02 neg","font":3]]></dct:description>
