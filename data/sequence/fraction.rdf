        <dct:description><![CDATA["gg":[100,2],"number":10,"time":0,"input":{"attr":[1.8,1.2,1.2],"digit":1,"values":["&lt;","=","&gt;"]},"regexp": { "output": "\\\\?" },"template":"template2.html","gen":"function($this,_s,_i){ var de=Math.floor(Math.random()*_s.gg[0]-2)+2, num=Math.floor(Math.random()*de*_s.gg[1]+1), op=num+' '+de+' / .mar 1 .mar .nop',tree = jtools.math.pol2tree(op), svg = jtools.math.tree2svg(tree, {}); var ratio = (5*svg.size[0])/(10*svg.size[1]); return [\\\"<div style='width:\\\"+Math.min(100,ratio*100)+\\\"%;margin:0 auto;'>\\\"+svg.svg+'</div>', (num/de==1)?\\\"=\\\":((num/de<1)?\\\"&lt;\\\":\\\"&gt;\\\")];}","class":"alpha p02 neg","font":3]]></dct:description>
