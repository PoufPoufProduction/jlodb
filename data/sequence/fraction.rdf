        <dct:title xml:lang="fr-FR">Comparer, sans contrainte de temps, le nombre de parts sélectionnées de 2 disques divisés également en 2.</dct:title>
        <dct:description><![CDATA["gg":{"d":[2,3,4,5,6,8],"v":true,"c":[true,true]},"font":1.5,"number":15,"time":0,"input":{"attr":[1.8,1.2,1.2],"digit":1,"values":["&lt;","=","&gt;"]},"regexp": { "output": "\\\\.\\\\.\\\\." },"template":"template2.html","gen":"function($this,_s,_i){var de1=_s.gg.d[Math.floor(Math.random()*_s.gg.d.length)], de2=_s.gg.d[Math.floor(Math.random()*_s.gg.d.length)], num1=Math.floor(Math.random()*de1+1),  num2=Math.floor(Math.random()*de2+1), circle=function(_num,_de) { var ret=''; if (_num==_de) { ret+=\\\"<circle cx='24' cy='24' r='22' class='f'/>\\\"; } else { ret+=\\\"<path d='M24,24 v-22 a22,22 0 \\\"+(_num>_de/2?1:0)+\\\",1 \\\"+(22*Math.sin(2*Math.PI*_num/_de))+\\\",\\\"+(22-22*Math.cos(2*Math.PI*_num/_de))+\\\" z' class='f'/>\\\"; }for (var i=0;i<_de;i++) { ret+=\\\"<line x1='24' y1='24' x2='\\\"+(24+22*Math.sin(2*Math.PI*i/_de))+\\\"' y2='\\\"+(24-22*Math.cos(2*Math.PI*i/_de))+\\\"' class='s'/>\\\"; } ret+=\\\"<circle cx='24' cy='24' r='22' class='s'/>\\\"; return ret; }, square=function(_num,_de) { var cols=(_de%2==0),rows=cols?_de/2:_de; var ret=''; for (var i=0;i<_num;i++) { var w=cols?22:44, h=cols?88/_de:44/_de, x=2+22*(cols?i%2:0), y=2+h*(cols?Math.floor(i/2):i); ret+=\\\"<rect x='\\\"+x+\\\"' y='\\\"+y+\\\"' width='\\\"+w+\\\"' height='\\\"+h+\\\"' class='f'/>\\\"; } if (cols) { ret+=\\\"<line x1='24' y1='2' x2='24' y2='46' class='s'/>\\\" } for (var i=1;i<rows;i++) { ret+=\\\"<line x1='2' y1='\\\"+(2+44*i/rows)+\\\"' x2='46' y2='\\\"+(2+44*i/rows)+\\\"' class='s'/>\\\"; } ret+=\\\"<rect x='2' y='2' width='44' height='44' class='s'/>\\\"; return ret; }; var ret=\\\"<div style='width:90%'><svg xmlns='http://www.w3.org/2000/svg' width='100%' height='100%' viewBox='0 0 \\\"+(48*(_s.gg.v?5:3))+\\\" 48' id='sefra'><def><style>svg#sefra .s {fill:none;stroke:black;stroke-width:0.5;}svg#sefra .f{fill:red;}.good svg#sefra .f{fill:green;}.wrong svg#sefra .f{fill:red;}svg#sefra text{font-size:24px;text-align:center;text-anchor:middle}</style></def>\\\"; var t=0,oo=0; for (var i=0; i<50; i++) { var tt=Math.floor(Math.random()*_s.gg.c.length); if (_s.gg.c[tt]) { t=tt; } } ret+=\\\"<g transform='translate(\\\"+((oo++)*48)+\\\",0)'>\\\"+(t==0?circle(num1,de1):square(num1,de1))+\\\"</g>\\\";  if (_s.gg.v) { ret+=\\\"<g transform='translate(\\\"+(24+(oo++)*48)+\\\",0)'><path d='M -20,24 20,24' style='fill:none;stroke:black;'/>\\\"; ret+=\\\"<text transform='translate(0,20)'>\\\"+num1+\\\"</text><text transform='translate(0,44)'>\\\"+de1+\\\"</text>\\\"; ret+='</g>'; } ret+=\\\"<g transform='translate(\\\"+(24+(oo++)*48)+\\\",0)'><text transform='translate(0,32)'>...</text></g>\\\"; if (_s.gg.v) { ret+=\\\"<g transform='translate(\\\"+(24+(oo++)*48)+\\\",0)'><path d='M -20,24 20,24' style='fill:none;stroke:black;'/>\\\"; ret+=\\\"<text transform='translate(0,20)'>\\\"+num2+\\\"</text><text transform='translate(0,44)'>\\\"+de2+\\\"</text>\\\"; ret+='</g>'; } ret+=\\\"<g transform='translate(\\\"+((oo++)*48)+\\\",0)'>\\\"+(t==0?circle(num2,de2):square(num2,de2))+\\\"</g>\\\"; ret+='</svg></div>'; return [ret, Math.round(100*num1/de1)==Math.round(100*num2/de2)?'=':(num1/de1<num2/de2?'&lt;':'&gt;')];}","class":"alpha p02 neg"]]></dct:description>
