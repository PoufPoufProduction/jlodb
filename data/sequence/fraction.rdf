        <dct:description><![CDATA["gg":{"d":[2,3,4,5,6,8],"n":2,"c":[true,true],"q":[true,true]},"font":3,"number":15,"time":0,"input":{"digit":2,"speed":5000},"regexp":{"output":"\\\\?"},"gen":"function($this,_s,_i){var de=_s.gg.d[Math.floor(Math.random()*_s.gg.d.length)], num=Math.floor(Math.random()*_s.gg.n*de+1), circle=function(_num,_de) { var ret=''; if (_num==_de) { ret+=\\\"<circle cx='24' cy='24' r='22' class='f'/>\\\"; } else { ret+=\\\"<path d='M24,24 v-22 a22,22 0 \\\"+(_num>_de/2?1:0)+\\\",1 \\\"+(22*Math.sin(2*Math.PI*_num/_de))+\\\",\\\"+(22-22*Math.cos(2*Math.PI*_num/_de))+\\\" z' class='f'/>\\\"; }for (var i=0;i<_de;i++) { ret+=\\\"<line x1='24' y1='24' x2='\\\"+(24+22*Math.sin(2*Math.PI*i/_de))+\\\"' y2='\\\"+(24-22*Math.cos(2*Math.PI*i/_de))+\\\"' class='s'/>\\\"; } ret+=\\\"<circle cx='24' cy='24' r='22' class='s'/>\\\"; return ret; }, square=function(_num,_de) { var cols=(_de%2==0),rows=cols?_de/2:_de; var ret=''; for (var i=0;i<_num;i++) { var w=cols?22:44, h=cols?88/_de:44/_de, x=2+22*(cols?i%2:0), y=2+h*(cols?Math.floor(i/2):i); ret+=\\\"<rect x='\\\"+x+\\\"' y='\\\"+y+\\\"' width='\\\"+w+\\\"' height='\\\"+h+\\\"' class='f'/>\\\"; } if (cols) { ret+=\\\"<line x1='24' y1='2' x2='24' y2='46' class='s'/>\\\" } for (var i=1;i<rows;i++) { ret+=\\\"<line x1='2' y1='\\\"+(2+44*i/rows)+\\\"' x2='46' y2='\\\"+(2+44*i/rows)+\\\"' class='s'/>\\\"; } ret+=\\\"<rect x='2' y='2' width='44' height='44' class='s'/>\\\"; return ret; }; var ret=\\\"<svg xmlns='http://www.w3.org/2000/svg' width='100%' height='100%' viewBox='0 0 \\\"+(48*(_s.gg.n+1))+\\\" 48' id='sefra'><def><style>svg#sefra .s {fill:none;stroke:black;stroke-width:0.5;}svg#sefra .f{fill:red;}.good svg#sefra .f{fill:green;}.wrong svg#sefra .f{fill:red;}svg#sefra text{font-size:24px;text-align:center;text-anchor:middle}</style></def>\\\"; var n=num,t=0,q=0; for (var i=0; i<50; i++) { var tt=Math.floor(Math.random()*_s.gg.c.length),qq=Math.floor(Math.random()*_s.gg.q.length); if (_s.gg.c[tt]) { t=tt; } if (_s.gg.q[qq]) { q=qq; }} for (var i=0; i<_s.gg.n;i++) { ret+=\\\"<g transform='translate(\\\"+(i*48)+\\\",0)'>\\\"+(t==0?circle(Math.min(n,de),de):square(Math.min(n,de),de))+\\\"</g>\\\"; n=Math.max(0,n-de); } ret+=\\\"<g transform='translate(\\\"+(24+_s.gg.n*48)+\\\",0)'><path d='M -20,24 20,24' style='fill:none;stroke:black;'/>\\\"; ret+=\\\"<text transform='translate(0,20)'>\\\"+(q?num:'?')+\\\"</text><text transform='translate(0,44)'>\\\"+(q?'?':de)+\\\"</text>\\\"; ret+='</g>'; ret+='</svg>'; return [ret, q?de:num];}","class":"alpha p02 blank"]]></dct:description>
