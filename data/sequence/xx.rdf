    <rdf:Description>
        <dct:identifier>t1</dct:identifier>
        <dct:title xml:lang="fr-FR">Exercice numérique simple avec génération automatique.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/c_yellow04.svg","background2":"res/img/background/landscape/c_blue01.svg","class":"h12","input":{"digit":2,"speed":5000},"gen":"function() { var a = 10+Math.floor(Math.random()*80),b; do { b = 10+Math.floor(Math.random()*80); } while(a+b>=100 || a%10+b%10>9); return \\\"\\\"+a+\\\"+\\\"+b; }","font":1.8]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"comment":"Exercice de contrôle [gen]"]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>root</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>t1</dct:alternative>
        <dct:coverage xml:lang="fr-FR">test</dct:coverage>
        <dct:source>res/img/background/landscape/c_yellow04.svg,res/img/background/landscape/c_blue01.svg</dct:source>
    </rdf:Description>

