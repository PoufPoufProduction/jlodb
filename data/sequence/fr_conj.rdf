    <rdf:Description>
        <dct:identifier>_cini01</dct:identifier>
        <dct:title xml:lang="fr-FR">Compléter la terminaison de l'infinif des verbes.</dct:title>
        <dct:description><![CDATA["time":0,"regexp": { "output": "\\\\.\\\\.\\\\." },"values":[["march...","er"],["se gliss...","er"],["entr...","er"],["parl...","er"],["se sauv...","er"],["cass...","er"],["mang...","er"],["se lav...","er"],["cri...","er"],["pleur...","er"],["fin...","ir"],["sort...","ir"],["part...","ir"],["dorm...","ir"],["bond...","ir"],["surg...","ir"],["cour...","ir"],["atterr...","ir"],["gém...","ir"],["s'enfu...","ir"],["boi...","re"],["prend...","re"],["vend...","re"],["remett...","re"],["comprend...","re"],["surprend...","re"],["se tai...","re"],["détend...","re"],["peind...","re"],["joind...","re"]],"class":"alpha h12 p02","font":1.2]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"input":{"attr":[1,2.7,1.2],"digit":1,"values":[["-er","er"],["-ir","ir"],["-re","re"]]},"comment":"[b]Complétez[/b] la terminaison de l'[red]infinif[/red] des verbes."]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>frconj</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>_cini01</dct:alternative>
		<dct:coverage xml:lang="fr-FR">infinitif</dct:coverage>
    </rdf:Description>
	
    <rdf:Description>
        <dct:identifier>_cint01</dct:identifier>
        <dct:title xml:lang="fr-FR">Déterminer le temps d'une phrase simple.</dct:title>
        <dct:description><![CDATA["time":0,"number":15,"values":[["Aussitôt, entre voisins, le caquetage [b]commence[/b].","p"],["Elle [b]demeure[/b] rue de l'Homme-Armé, numéro sept.","p"],["Ta console [b]remporte[/b] un grand succès.","p"],["Nous [b]regardons[/b] un film passionnant.","p"],["Ils ne [b]sont[/b] pas chers, mais ils [b]sont[/b] admirables.","p"],["L'affaire [b]fait[/b] déjà grand bruit.","p"],["L'air [b]est[/b] lourd, la [b]mer[/b] est calme.","p"],["On [b]fait[/b] cercle autour de son bureau.","p"],["Après ma chute, j'[b]avais perdu[/b] beaucoup de sang.","a"],["Arrêtée, un bol à la main, elle le [b]regardait[/b].","a"],["Tout le monde [b]a joué[/b] avec ta nouvelle console.","a"],["Les acteurs [b]savaient[/b] leur rôle par cœur. ","a"],["Il [b]pleuvait[/b] un peu dans les arbres déjà verts.","a"],["La neige [b]continuait[/b] de tomber; mais voilà tout.","a"],["La fenêtre [b]donnait[/b] sur une place.","a"],["On [b]discutait[/b], on [b]chantait[/b], on [b]riait[/b].","a"],["A dix heures, la lune ne [b]sera[/b] pas encore levée.","f"],["Ma grand-mère [b]passera[/b] les fêtes de Noël avec nous.","f"],["J’[b]aurai[/b] du mal à la récupérer.","f"],["Je ne me [b]tairai[/b] pas !","f"],["Je vous [b]ferai[/b] gronder par monsieur Gillenormand.","f"],["Nous [b]passerons[/b] sans doute la nuit dans la cave.","f"],["Les premiers arrivés [b]seront[/b] les mieux placés.","f"],["Quand tu [b]seras[/b] plus grand, tu [b]feras[/b] autre chose.","f"]],"class":"vertical alpha h12 p05","font":0.35]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"input":{"attr":[2.5,4,1.2],"digit":1,"values":[["présent","p"],["passé","a"],["futur","f"]]},"comment":"[b]Déterminez[/b] le [red]temps[/red] des phrase proposées."]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>frconj</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>_cint01</dct:alternative>
    </rdf:Description>
	
	
