    <rdf:Description>
        <dct:identifier>t01</dct:identifier>
        <dct:title xml:lang="fr-FR">Associer chaque valeur à son type.</dct:title>
        <dct:description><![CDATA["values":[["1","valeur numérique"],["42","valeur numérique"],["65","valeur numérique"],["3.1415","valeur numérique"],["-12","valeur numérique"],["9999","valeur numérique"],["'A'","caractère"],["'_'","caractère"],["'1'","caractère"],["'0'","caractère"],["'@'","caractère"],["\\\"HELLO\\\"","chaîne de caractères"],["\\\"A\\\"","chaîne de caractères"],["\\\"1\\\"","chaîne de caractères"],["\\\"0\\\"","chaîne de caractères"],["\\\"(!@Eq#\\\"","chaîne de caractères"],["[324,0.5]","tableau"],["[1,2,3]","tableau"],["[1]","tableau"],["[0]","tableau"],["['A','B','C']","tableau"]],"bgcolor":["rgba(180,180,180,0.9)","rgba(255,252,246,0.9)"],"class":"h12","font":0.8,"len":6,"nbvalues":6,"number":2]]></dct:description>
