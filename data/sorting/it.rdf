    <rdf:Description>
        <dct:identifier>t02</dct:identifier>
        <dct:title xml:lang="fr-FR">Associer chaque commande à son résultat [PRINT,DUMP].</dct:title>
        <dct:description><![CDATA["values":[["PRINT('0')","0"],["PRINT(0)","."],["DUMP('0')","48"],["DUMP(0)","0"],["PRINT('1')","1"],["PRINT(1)","."],["DUMP('1')","49"],["DUMP(1)","1"],["PRINT('A')","A"],["PRINT(65)","A"],["DUMP('A')","65"],["DUMP(65)","65"],["PRINT('B')","B"],["PRINT(66)","B"],["DUMP('B')","66"],["DUMP(66)","66"]],"bgcolor":["rgba(180,180,180,0.9)","rgba(255,252,246,0.9)"],"class":"h12","font":0.9,"len":10,"nbvalues":6,"number":2]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Associez chaque [b]commande[/b] à son [b]résultat[/b].[br]Quelques valeurs [blue]ASCII[/blue] utiles :[br]['0':48] ['1':49] ['A':65] ['B':66].[br][i]Le point représente un caractère non affichable[/i].","fontex":0.6]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>t01</dct:alternative>
    </rdf:Description>
    
    
    <rdf:Description>
