        <dct:title xml:lang="fr-FR">Associer à chaque couleur son codage RGB.</dct:title>
        <dct:description><![CDATA["class":"h12","gen":"function(){ var num=1, r=Math.min(255,Math.floor(Math.random()*(num+1))*256/num),g=Math.min(255,Math.floor(Math.random()*(num+1))*256/num),b=Math.min(255,Math.floor(Math.random()*(num+1))*256/num);  return ['<div style=\\\"width:5em;height:1.2em;background-color:rgb('+r+','+g+','+b+')\\\"></div>','('+r+','+g+','+b+')'];}","nbvalues":5,"number":2,"font":1.4,"len":4]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Associez à chaque couleur son codage [b]RGB[/b].","fontex":0.7]]></dct:description>
