 	<rdf:Description>
        <dct:identifier>l01</dct:identifier>
        <dct:title xml:lang="fr-FR">Associer chaque comparaison à sa description.</dct:title>
        <dct:description><![CDATA["len":3]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"values":[["1<2","1inf2"],["1<3","1inf3"],["1<4","1inf4"],["1<5","1inf5"],["2<3","2inf3"],["2<4","2inf4"],["2<5","2inf5"],["3<4","3inf4"],["3<5","3inf5"],["4<5","4inf5"],["5>4","5sup4"],["5>3","5sup3"],["5>2","5sup2"],["5>1","5sup1"],["4>3","4sup3"],["4>2","4sup2"],["4>1","4sup1"],["3>2","3sup2"],["3>1","3sup1"],["2>1","2sup1"]],"nbvalues":5,"number":3,"class":"h12","font":1.2,"regexp":{"response":{ "from": "(.)(.{3})(.)","to":"[b]$1[/b] est [i]$2érieur[/i] à [b]$3[/b]"}},"exercice":"[b]Associez[/b] chaque comparaison à sa description.","fontex":0.5]]></dct:description>
        <dct:extent>2</dct:extent>
