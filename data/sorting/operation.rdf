    <rdf:Description>
        <dct:identifier>10</dct:identifier>
        <dct:title xml:lang="fr-FR">Associer les soustractions, additions, multiplications jusqu'à 5 et divisions par 2, de valeurs comprises entre 1 et 10 à leur résultat respectif.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/bluecarpet01.svg","gg":{"ad":[7,3,7,3],"su":[7,3,7,3],"mu":[4,2,10,1],"di":[9,2,1,2]},"class":"h12","gen":"function($this,_s,_id){val=[function() { var a=Math.floor(Math.random()*_s.gg.ad[0]+_s.gg.ad[1]),b=Math.floor(Math.random()*_s.gg.ad[2]+_s.gg.ad[3]); return [a+'+'+b,(a+b)]; }, function() { var a = Math.floor(Math.random()*_s.gg.su[0]+_s.gg.su[1]), b = Math.floor(Math.random()*_s.gg.su[2]+_s.gg.su[3]); return [(a+b)+'-'+b,a]; }, function() { var a=Math.floor(Math.random()*_s.gg.mu[0]+_s.gg.mu[1]),b = Math.floor(Math.random()*_s.gg.mu[2]+_s.gg.mu[3]); return [a+'×'+b,(a*b)];}, function() { var a = Math.floor(Math.random()*_s.gg.di[0]+_s.gg.di[1]), b = Math.floor(Math.random()*_s.gg.di[2]+_s.gg.di[3]); return [(a*b)+'/'+b,a]; }], v=Math.floor(Math.random()*val.length); return val[v](); }","nbvalues":5,"number":3,"len":4,"font":1.5]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>operations</dct:subject>
