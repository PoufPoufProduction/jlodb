    <rdf:Description>
        <dct:identifier>10</dct:identifier>
        <dct:title xml:lang="fr-FR">Associer la soustraction d'un nombre compris entre 1 et 10 à son résultat compris également entre 1 et 10.</dct:title>
        <dct:description><![CDATA["gen":"function(i){var a = Math.floor(Math.random()*10)+1, b = Math.floor(Math.random()*10)+1; return [\\\"\\\"+(a+b)+\\\"-\\\"+b,a];}","nbvalues":5,"number":5,"len":4,"font":1.7]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>subtraction</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>10</dct:alternative>
    </rdf:Description>

