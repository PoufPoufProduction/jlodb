    <!-- words and nouns -->

    <rdf:Description>
        <dct:identifier>gw001</dct:identifier>
        <dct:title xml:lang="fr-FR">Souligner les noms communs présents dans le texte.</dct:title>
        <dct:description><![CDATA["text":["La |récréation| est finie.","La |mer| est calme.","On veut des |frites| !","Les |nuages| cachent le |soleil|.","Les |carottes| sont cuites."],"questions":[{"label":"nom commun","s":"|"}],"font":1]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Coloriez en bleu les [b]noms communs[/b] présents dans le texte."]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>frgram</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>gw001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>gw002</dct:identifier>
        <dct:title xml:lang="fr-FR">Souligner les noms communs présents dans le texte.</dct:title>
        <dct:description><![CDATA["text":["Le petit |chat| boit du |lait|.","Le |garçon| court dans le |jardin|.","J'aime faire du |vélo|.","Mon |oncle| lit le |journal|.","Parfois, le vieil |homme| s'ennuie."],"questions":[{"label":"nom commun","s":"|"}],"font":1]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Coloriez en bleu les [b]noms communs[/b] présents dans le texte."]]></dct:description>
