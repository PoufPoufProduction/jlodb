    <!-- gender -->

   <rdf:Description>
        <dct:identifier>gg001</dct:identifier>
        <dct:title xml:lang="fr-FR">Repérer dans le texte les noms masculins et les noms féminins précédés des articles 'le','la','un' ou 'une'.</dct:title>
        <dct:description><![CDATA["text":["La $récréation$ est finie.","La $mer$ est calme.","J'ai vu un |oiseau| sur une $branche$.","Le |soleil| se cache derrière un |nuage|.","Le |lapin| a mangé une $carotte$."],"questions":[{"label":"nom masculin","s":"|"},{"label":"nom féminin","s":"$"}],"font":0.9]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Coloriez en bleu les [b]noms masculins[/b] et en rouge les [b]noms féminins[/b]."]]></dct:description>
