        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>gw001</dct:alternative>
    </rdf:Description>


    <rdf:Description>
        <dct:identifier>gw051</dct:identifier>
        <dct:title xml:lang="fr-FR">Repérer les noms communs et propres présents dans le texte.</dct:title>
        <dct:description><![CDATA["text":["$Marie$ mange une |pomme|.","$Amélie$ nage très bien.","Mes |amis| s'appellent $Pierre$ et $Richard$.","$Thomas$ a de la |chance|.","Hier, j'ai vu $Gaston$ à la |piscine|.","$Londres$ est une grande |ville|."],"questions":[{"label":"nom commun","s":"|"},{"label":"nom propre","s":"$"}],"font":0.8]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Coloriez en bleu les [b]noms communs[/b] et en rouge les [b]noms propres[/b]."]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>frgram</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>gw001</dct:alternative>
    </rdf:Description>


    <!-- article -->


   <rdf:Description>
        <dct:identifier>ga001</dct:identifier>
