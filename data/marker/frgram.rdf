        <dct:subject>frgram</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>gg001</dct:alternative>
    </rdf:Description>

   <rdf:Description>
        <dct:identifier>gg002</dct:identifier>
        <dct:title xml:lang="fr-FR">Repérer dans le texte les noms masculins et les noms féminins précédés des articles 'le','la','un' ou 'une'.</dct:title>
        <dct:description><![CDATA["text":["Une $voiture$ rouge passe sur le |pont|.","A la $ferme$, j'ai vu 3 $poules$ et 2 $vaches$.","Une $souris$ a mangé le |fromage|.","Le |tailleur| a cousu un beau |pantalon|.","Le |gendarme| court après un |voleur|."],"questions":[{"label":"nom masculin","s":"|"},{"label":"nom féminin","s":"$"}],"font":0.9]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Coloriez en bleu les [b]noms masculins[/b] et en rouge les [b]noms féminins[/b]."]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>frgram</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>gg001</dct:alternative>
    </rdf:Description>

   <rdf:Description>
        <dct:identifier>gg011</dct:identifier>
        <dct:title xml:lang="fr-FR">Repérer dans le texte les noms masculins et les noms féminins précédés des articles 'les','l', ou 'des'.</dct:title>
        <dct:description><![CDATA["text":["Des |oiseaux| ont mangé les $graines$.","L'$autruche$ a des grandes $plumes$.","Les |enfants| aiment les $sucreries$.","Les |nuages| cachent les $étoiles$.","J'ai mal à l'$oreille$."],"questions":[{"label":"nom masculin","s":"|"},{"label":"nom féminin","s":"$"}],"font":0.9]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Coloriez en bleu les [b]noms masculins[/b] et en rouge les [b]noms féminins[/b]."]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>frgram</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>gg001</dct:alternative>
    </rdf:Description>

