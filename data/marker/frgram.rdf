   <rdf:Description>
        <dct:identifier>ga011</dct:identifier>
        <dct:title xml:lang="fr-FR">Souligner les articles présents dans le texte.</dct:title>
        <dct:description><![CDATA["text":["|La| récréation est finie.","|La| mer est calme.","Marie mange |une| pomme.","|Les| nuages cachent |le| soleil.","|Les| carottes sont cuites."],"questions":[{"label":"articles","s":"|"}],"font":1]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Coloriez en bleu les [b]articles[/b] présents dans le texte."]]></dct:description>
