        <dct:identifier>s5100</dct:identifier>
        <dct:title xml:lang="fr-FR">Compléter un tableau de 5 nombres (compris entre 100 et 1000) variant de 100 en 100.</dct:title>
        <dct:description><![CDATA["gg":[500,100,5,100,3,[0,2]],"background":"res/img/background/landscape/graycarpet01.svg","keypad":"num0","hint":false,"horiz":true,"gen":"function($this,_s,_i){var a=Math.floor(Math.random()*_s.gg[0])+_s.gg[1],data=[],fixed=[];for (var i=0;i<_s.gg[2];i++){data.push((a+i*_s.gg[3]).toString());} for (var j in _s.gg[5]) for (var i=0; i<_s.gg[4]; i++) { fixed.push([i,_s.gg[5][j]]); } return {'data':data,'fixed':fixed}}"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"title":"[strong]100[/strong]","exercice":"[b]Complétez[/b] le tableau en renseignant les nombres variant de [b]100[/b] en [b]100[/b].","hex":16,"fontex":1.1,"hfiller":28]]></dct:description>
