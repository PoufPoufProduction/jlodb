    <rdf:Description>
        <dct:identifier>s5100</dct:identifier>
        <dct:title xml:lang="fr-FR">Compléter un tableau de 5 nombres (compris entre 100 et 1000) variant de 100 en 100.</dct:title>
        <dct:description><![CDATA["gg":{"range":[400,100],"size":[3,6],"step":[0,100],"stepfactor":1,"fixed":[0,1,2]},"background":"res/img/background/landscape/c_gray02.svg","keypad":"num0","hint":false,"horiz":true,"gen":"function($this,_s,_i){var a=Math.floor(Math.random()*_s.gg.range[0])+_s.gg.range[1],data=[],fixed=[],step; do { step=(Math.floor(Math.random()*_s.gg.step[0])+_s.gg.step[1])*_s.gg.stepfactor } while (step==0); for (var i=0;i<_s.gg.size[1];i++){ var v=(a+i*step).toString(); while(v.length<_s.gg.size[0]) { v='_'+v; } data.push(v); } for (var j in _s.gg.fixed) for (var i=0; i<_s.gg.size[0]; i++) { fixed.push([i,_s.gg.fixed[j]]); } return {'data':data,'fixed':fixed}}","emptyval":"_"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"[b]Observez[/b] et [b]complétez[/b] la série de nombres.","hex":44,"fontex":1.1]]></dct:description>
