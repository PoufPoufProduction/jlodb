    <!-- MERE ITERATION -->

    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Afficher 100 fois la lettre 'A' sur la sortie standard.</dct:title>
        <dct:description><![CDATA["class":"l2","a":{"op":["print"],"va":["V100","VA"],"cl":["rep"]},"valid":{"stdout":-2004395608}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["[b]Affichez 100 fois la lettre [i]'A'[/i] sur la sortie standard.[/b]","","Il est fortement conseillé d'utiliser la fonction [red][b]REPEAT[/b][/red] disponible dans le 4ème onglet qui permet de répeter une opération un certain nombre de fois. Le nombre d'itérations est précisé par le paramètre situé à droite du libellé.","","[strong]Pour que le programme ne dure pas trop longtemps, il est également conseillé d'en changer sa vitesse d'exécution.[/strong]"]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

