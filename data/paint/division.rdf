   <!-- DIVIDE and MULTIPLE -->

    <rdf:Description>
        <dct:identifier>da1</dct:identifier>
        <dct:title xml:lang="fr-FR">Colorier dans une grille 10x10 tous les diviseurs d'un nombre compris entre 10 et 99.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/bluesplash01.svg","url":"res/img/desktop/paint/numbers10x10.svg","data":[{"skip":true},{"toggle":true}],"selected":1,"number":2,"svgclass":"nozero s1","gen":"function(){var result, nb; do { nb=0, result=' '; var a=Math.floor(Math.random()*90)+10; for (var i=1; i<100; i++) { if (i>a||a%i) { result+=' '; } else { result+='1'; nb++;} } } while(nb<4); return {'t':['/'+a.toString()],'result':result};}","remove":true]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"[b]Coloriez[/b] TOUS les [red]diviseurs[/red] du nombre ci-dessus."]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>da1</dct:alternative>
        <dct:source>res/img/desktop/paint/numbers10x10.svg,res/img/background/landscape/bluesplash01.svg</dct:source>
    </rdf:Description>
	
	<rdf:Description>
        <dct:identifier>db1</dct:identifier>
        <dct:title xml:lang="fr-FR">Colorier dans une grille 10x10 tous les multiples d'un nombre compris entre 2 et 99.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/bluesplash01.svg","url":"res/img/desktop/paint/numbers10x10.svg","data":[{"skip":true},{"toggle":true}],"selected":1,"number":2,"svgclass":"nozero s1","gen":"function(){var result, nb; do { nb=0, result=' '; var a=Math.floor(Math.random()*98)+2; for (var i=1; i<100; i++) { if (a>i||i%a) { result+=' '; } else { result+='1'; nb++;} } } while(nb<4); return {'t':['×'+a.toString()],'result':result};}","remove":true]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"[b]Coloriez[/b] TOUS les [red]multiples[/red] du nombre ci-dessus."]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>da1</dct:alternative>
        <dct:source>res/img/desktop/paint/numbers10x10.svg,res/img/background/landscape/bluesplash01.svg</dct:source>
    </rdf:Description>
	

    <rdf:Description>
        <dct:identifier>da2</dct:identifier>
        <dct:title xml:lang="fr-FR">Colorier dans une grille 10x10 les diviseurs de 2 nombres compris entre 10 et 100.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/bluesplash01.svg","url":"res/img/desktop/paint/numbers10x10.svg","data":[{"skip":true},{"toggle":true},{"toggle":true},{"toggle":true}],"selected":1,"svgclass":"nozero s1 s2 s3","gen":"function(){var result,a,b; do { nb=[0,0,0], result=' '; var a=Math.floor(Math.random()*90)+10, b=Math.floor(Math.random()*90)+10; for (var i=1; i<100; i++) { if (i>a||a%i) { if (i>b||b%i) { result+=' '; } else { result+='2'; nb[1]++; } } else { if (i>b||b%i) { result+='1'; nb[0]++} else { result+='3'; nb[2]++; } } } } while (nb[0]<4 || nb[1]<4); return {'result':result,'t':['/'+a.toString(),'/'+b.toString(),'/']};}","remove":true]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Pour chacun des deux nombres, [b]coloriez[/b] TOUS les [red]diviseurs[/red] suivant leur couleur. [blue]Coloriez en bleu les diviseurs communs.[/blue]","fontex":0.8]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>da1</dct:alternative>
        <dct:source>res/img/desktop/paint/numbers10x10.svg,res/img/background/landscape/bluesplash01.svg</dct:source>
    </rdf:Description>
	
    <rdf:Description>
        <dct:identifier>db2</dct:identifier>
        <dct:title xml:lang="fr-FR">Colorier dans une grille 10x10 les multiples de 2 nombres compris entre 2 et 100.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/bluesplash01.svg","url":"res/img/desktop/paint/numbers10x10.svg","data":[{"skip":true},{"toggle":true},{"toggle":true},{"toggle":true}],"selected":1,"svgclass":"nozero s1 s2 s3","gen":"function(){var result,a,b; do { nb=[0,0,0], result=' '; var a=Math.floor(Math.random()*98)+2, b=Math.floor(Math.random()*98)+2; for (var i=1; i<100; i++) { if (a>i||i%a) { if (b>i||i%b) { result+=' '; } else { result+='2'; nb[1]++; } } else { if (b>i||i%b) { result+='1'; nb[0]++} else { result+='3'; nb[2]++; } } } } while (nb[0]<4 || nb[1]<4 || nb[2]<1); return {'result':result,'t':['×'+a.toString(),'×'+b.toString(),'×']};}","remove":true]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Pour chacun des deux nombres, [b]coloriez[/b] TOUS les [red]multiples[/red] suivant leur couleur. [blue]Coloriez en bleu les multiples communs.[/blue]","fontex":0.8]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>da1</dct:alternative>
        <dct:source>res/img/desktop/paint/numbers10x10.svg,res/img/background/landscape/bluesplash01.svg</dct:source>
    </rdf:Description>
	
