    <rdf:Description>
        <dct:identifier>d001</dct:identifier>
        <dct:title xml:lang="fr-FR">Colorier dans une grille 10x10 tous les diviseurs d'un nombre compris entre 10 et 99.</dct:title>
        <dct:description><![CDATA["url":"res/img/desktop/paint/numbers10x10.svg","data":[{"skip":true},{"toggle":true}],"selected":1,"number":3,"class":"nozero nosource","gen":"function(){var result=' ',a=Math.floor(Math.random()*90)+10; for (var i=1; i<100; i++) { result+=(i>a||a%i)?' ':'1'; } return {'tag':a.toString(),'result':result};}","remove":true]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Coloriez TOUS les diviseurs du nombre suivant."]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>d001</dct:alternative>
        <dct:source>res/img/desktop/paint/numbers10x10.svg</dct:source>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>d101</dct:identifier>
        <dct:title xml:lang="fr-FR">Colorier dans une grille 10x10 les diviseurs de 45 et 49.</dct:title>
        <dct:description><![CDATA["url":"res/img/desktop/paint/numbers10x10.svg","data":[{"skip":true},{"toggle":true},{"toggle":true},{"toggle":true}],"selected":1,"class":"nozero","gen":"function(){var a=45,b=49,result=' '; for (var i=1; i<100; i++) { if (i>a||a%i) {result+=(i>b||b%i)?' ':'2';} else {result+=(i>b||b%i)?'1':'3';} } return {'result':result,'t':[a.toString(),b.toString(),a.toString()+' '+b.toString()]};}","remove":true]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Coloriez en <span style='color:red;'>rouge</span> les diviseurs de <b>45</b>, en <span style='color:green;'>vert</span> les diviseurs de <b>49</b> et en <span style='color:blue;'>bleu</span> les diviseurs communs au deux nombres.","fontex":0.8]]></dct:description>
