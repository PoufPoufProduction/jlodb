        <dct:description><![CDATA["op":["plus"],"gen":"function($this,_s,_index){ var range=[30,10],nb=3,tiles=[],value=0,t=0; for (var i=0;i<6;i++) { tiles.push(Math.floor(Math.random()*(range[0]-range[1]))+range[1]); } for (var i=0;i<nb;i++) { var v; do { v=Math.floor(Math.random()*tiles.length); } while (t&(1<<v)); t=t|(1<<v); value+=tiles[v]; }  tiles.sort(function(a,b){return (parseInt(a)>parseInt(b));}); return {\\\"values\\\":tiles,\\\"result\\\":value} }","number":5]]></dct:description>
