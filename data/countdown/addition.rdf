        <dct:description><![CDATA["op":["plus"],"gen":"function($this,_s,_index){ var tt=[[4,5,6,7,8,9],[4,6,6,7,8,9],[5,6,7,7,8,8],[6,7,8,8,9,9]],tiles=tt[_index],a=Math.floor(Math.random()*tiles.length),b,value; do { var ok=true; do { b=Math.floor(Math.random()*tiles.length); } while (a==b); value=tiles[a]+tiles[b]; for(var i in tiles) { if (value==tiles[i]) { ok=false; }} } while(!ok); return {\\\"values\\\":tiles,\\\"result\\\":value} }","number":4]]></dct:description>
