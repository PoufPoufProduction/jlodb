        <dct:description><![CDATA["pospanel":[75,50],"class":"leftfx","gg":[[0,0,96],[0,64,0],[1,15,0,0,50,0],[0,1,55,0,2,25]],"background":"res/img/background/landscape/c_blue01.svg","emptyisnull":true,"toright":true,"errratio":2,"callen":2,"noneg":true,"nodec":true,"withbars":false,"sp":0.05,"cols":{"col0":{"width":0.2},"col1":{"width":11.5,"background":"#FEC"}},"rows":{"row0":{"height":0.2},"row1":{"background":"#AAA","color":"white","fixed":true}},"cells":{"c2x1":{"type":"free","value":"h"},"c3x1":{"type":"free","value":"min"},"c4x1":{"value":"s"},"c1x1":{"background":"gray"}},"gen":"function($this, _s, _id){var ret={}; for (var i=0;i<_s.gg.length;i++) { var v='',r=0,j=0,q=_s.gg[i]; while (j<q.length) { r+=3600*q[j]+60*q[j+1]+q[j+2]; if (j) { v+=' + '; } if (q[j]) { v+=q[j]+_s.po['h']+' '; } if (q[j+1]) { v+=q[j+1]+_s.po['min']+' '; } if (q[j+2]) { v+=q[j+2]+_s.po['s']+' '; } j+=3; } ret['c1x'+(i+2)]={'type':'value','value':v}; ret['c2x'+(i+2)]={'type':'value','result':Math.floor(r/3600)}; ret['c3x'+(i+2)]={'type':'value','result':Math.floor(r/60)%60}; ret['c4x'+(i+2)]={'type':'value','result':r%60};} return {'cells':ret}; }","font":1.1]]></dct:description>
