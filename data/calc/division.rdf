        <dct:description><![CDATA["gg":[[80,20],[40,40],[[1,10],[0.1,10],[10,0.1],[1,0.01],[1,0.1]]],"erratio":2,"sp":0.05,"background":"res/img/background/landscape/grayboard01.svg","font":1.5,"size":[2,6],"all":{"width":1.2,"height":1.2},"cols":{"col0":{"width":0.2},"col1":{"background":"#FDC","color":"#014","width":8},"col2":{"background":"#FEC","width":4.5}},"rows":{"row0":{"height":0.2}},"gen":"function($this,_s,_i){var c={},a,b; do { a=Math.floor(Math.random()*_s.gg[0][0])+_s.gg[0][1]; b=Math.floor(Math.random()*_s.gg[1][0])+_s.gg[1][1];  } while ((a*b)%10==0); c['c1x1']={'background':'#A30','color':'white','width':12.55,'type':'free','value':jtools.num.tostr(a*b)+jlo.div+jtools.num.tostr(b)+' = '+jtools.num.tostr(a),'fixed':true}; shuffle(_s.gg[2]); for (var j=0; j<5; j++) { c['c1x'+(j+2)]= {'type':'free','value':jtools.num.tostr(jtools.num.round(a*b*_s.gg[2][j][0]))+jlo.div+jtools.num.tostr(jtools.num.round(b*_s.gg[2][j][1]))}; c['c2x'+(j+2)]= {'type':'value','result':jtools.num.round(a*_s.gg[2][j][0]/_s.gg[2][j][1])}; } return { cells:c}; }","noneg":true,"nodec":false,"withbars":false,"withauto":false,"callen":6,"hide":[[2,1,1,1]]]]></dct:description>
