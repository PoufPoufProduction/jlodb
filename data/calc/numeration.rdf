    <rdf:Description>
        <dct:identifier>s25</dct:identifier>
        <dct:title xml:lang="fr-FR">Compléter des séries de nombres.</dct:title>
        <dct:description><![CDATA["checkempty":true,"callen":2,"noneg":true,"nodec":true,"sp":0.02,"rows":{"row0":{"height":0.2},"row9":{"height":0.2,"fixed":true,"background":"gray"}},"cols":{"col0":{"width":0.2},"col2":{"width":0.2,"fixed":true,"background":"gray"},"col4":{"width":0.2,"fixed":true,"background":"gray"},"col6":{"width":0.2,"fixed":true,"background":"gray"}},"withbars":false,"all":{"height":1.2,"width":2},"gen":"function() { var cells={},step=[2,3,4,5]; for (var i=0; i<3;i++) { var a=step[Math.floor(Math.random()*step.length)],b=Math.floor(Math.random()*19+2); for (var j=0; j<8;j++) { if (j<2 || j==7) { cells['c'+(2*i+1)+'x'+(j+1)]={'value':b+j*a,'fixed':true}; } else { cells['c'+(2*i+1)+'x'+(j+1)]={'result':b+j*a,'background':'#FDA','color':'#222'}; } } } return {'cells':cells}; }","font":1.2]]></dct:description>
