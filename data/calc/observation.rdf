    <rdf:Description>
        <dct:identifier>h55a</dct:identifier>
        <dct:title xml:lang="fr-FR">Lire un tableau à double entrée.</dct:title>
        <dct:description><![CDATA["class":"leftpanel","nonext":true,"noneg":true,"nodec":true,"tabs":["calc"],"sp":0.06,"hide":[[2,8,4,5]],"rows":{"row0":{"height":0.2},"row7":{"background":"red","height":0.2,"fixed":true}},"cols":{"col0":{"width":0.2}},"withbars":false,"all":{"height":1.2,"width":4.2},"gen":"function() { var w=5,h=5,cells={}, r=[[1,0],[3,1],[4,4],[0,0],[0,2]]; for (var j=0; j<h;j++) for (var i=0; i<w;i++) { var v=Math.floor(Math.random()*10); cells['c'+(i+2)+'x'+(j+2)]={'fixed':true,'value':v}; for (var k=0; k<r.length;k++) { if (r[k][0]==i && r[k][1]==j) { cells['c6x'+(k+8)]={'result':v,'background':'#AA8','color':'#FDA'}; }} } for (var j=0; j<h;j++){ cells['c1x'+(j+2)]={'type':'fixed','value':'n'+j,'background':'#AA8','color':'#FDA','fixed':true}; } for (var i=0; i<w;i++){ cells['c'+(i+2)+'x1']={'type':'fixed','value':'o'+i,'background':'#AA8','color':'#FDA','fixed':true}; } cells['c1x1']={'background':'#AA8','color':'#FDA','fixed':true}; cells['c1x'+(h+3)]={'type':'fixed','value':'q0','background':'white','width':21,'opt':'left'}; cells['c1x'+(h+4)]={'type':'fixed','value':'q1','background':'white','width':21,'opt':'left'}; cells['c1x'+(h+5)]={'type':'fixed','value':'q2','background':'white','width':21,'opt':'left'}; cells['c1x'+(h+6)]={'type':'fixed','value':'q3','background':'white','width':21,'opt':'left'}; cells['c1x'+(h+7)]={'type':'fixed','value':'q4','background':'white','width':21,'opt':'left'}; return {'cells':cells}; }","font":0.7]]></dct:description>
        <dct:description xml:lang="fr-FR">,"exercice":"En vous servant du tableau à double entrée qui décrit le contenu des trousses de chaque élève, [b]répondez aux questions ci-dessus[/b].","po":{"n0":"Paul","n1":"Claire","n2":"Sofia","n3":"Manon","n4":"Johann","o0":"Crayon","o1":"Feutre","o2":"Gomme","o3":"Stylo","o4":"Marqueur","q0":"Combien [b]Paul[/b] a-t'il de [b]feutres[/b] ?","q1":"Combien [b]Claire[/b] a-t'elle de [b]stylos[/b] ?","q2":"Combien [b]Johann[/b] a-t'il de [b]marqueurs[/b] ?","q3":"Combien [b]Paul[/b] a-t'il de [b]crayons[/b] ?","q4":"Combien [b]Sofia[/b] a-t'elle de [b]crayons[/b] ?"}</dct:description>
