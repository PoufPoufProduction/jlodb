        <dct:description><![CDATA["imgprefix":"res/img/clipart/animal/","withtabs":false,"img":["bear01","bird01","cat02","duck01","dog02","mice01","lama01","ibis01","swan01"],"sp":0.02,"rows":{"row0":{"height":0.2},"row3":{"type":"fixed","height":0.2,"background":"gray"},"row4":{"background":"#AA8"},"row5":{"background":"#AA8"}},"cols":{"col0":{"width":0.2}},"withbars":false,"all":{"height":1.2,"width":1.2},"gen":"function() { var w=4,h=2,cells={}; for (var j=0; j<h;j++) for (var i=0; i<w;i++) { var v=Math.floor(Math.random()*9); cells['c'+(i+1)+'x'+(j+1)]={'fixed':true,'type':'img','value':v}; cells['c'+(i+1)+'x'+(j+h+2)]={'type':'img','result':v}; } return {'cells':cells}; }","font":2.4]]></dct:description>
