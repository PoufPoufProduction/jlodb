        <dct:description><![CDATA["gg":{"ad":[7,3,7,3],"su":[7,3,7,3],"mu":[4,2,10,1],"di":[9,2,1,2]},"background":"res/img/background/landscape/c_gray02.svg","errratio":2,"checkempty":true,"callen":4,"noneg":true,"nodec":true,"sp":0.1,"rows":{"row0":{"height":0.2}},"cols":{"col0":{"width":0.2},"col1":{"fixed":true,"width":7.5},"col2":{"width":5}},"withbars":false,"all":{"height":1.2},"gen":"function($this, _s, _i) { var cells={},val=[function() { var a=Math.floor(Math.random()*_s.gg.ad[0]+_s.gg.ad[1]),b=Math.floor(Math.random()*_s.gg.ad[2]+_s.gg.ad[3]); return [a+'+'+b,(a+b)]; }, function() { var a = Math.floor(Math.random()*_s.gg.su[0]+_s.gg.su[1]), b = Math.floor(Math.random()*_s.gg.su[2]+_s.gg.su[3]); return [(a+b)+'-'+b,a]; }, function() { var a=Math.floor(Math.random()*_s.gg.mu[0]+_s.gg.mu[1]),b = Math.floor(Math.random()*_s.gg.mu[2]+_s.gg.mu[3]); return [a+'×'+b,(a*b)];}, function() { var a = Math.floor(Math.random()*_s.gg.di[0]+_s.gg.di[1]), b = Math.floor(Math.random()*_s.gg.di[2]+_s.gg.di[3]); return [(a*b)+'/'+b,a]; }]; for (var i=0; i<6; i++) { var v=Math.floor(Math.random()*val.length), vv=val[v](); cells['c1x'+(i+1)]={'fixed':true,'value':vv[0],'background':'#FEC','color':'#444'}; cells['c2x'+(i+1)]={'result':vv[1]}; } return {'cells':cells}; }","font":1.5]]></dct:description>
