    <!-- SEGMENTS AND LINES -->

    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser la barre d'outils pour tracer un trait à partir de 2 points.</dct:title>
        <dct:description><![CDATA["data":[{"type":"point", "value":[-150,0], "id":"A", "active":true}, {"type":"point", "value":[150,0], "id":"B", "active":true}],"available":["segment"],"number":1,"objectives":[[{"type":"segment", "values":[-150,0,150,0]}]],"translate":[320,185.5]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"statement":["Pour tracer un trait droit entre A et B, sélectionnez l'outil <b>trait</b> dans le menu à gauche, cliquez ensuite sur le point A, puis, en gardant le doigt ou le bouton de la souris appuyé, reliez le point B. Lorsque la couleur change, relachez la pression pour tracer le trait demandé.","[strong]Le trait droit qui relie les points A et B est appelé le segment AB.[/strong]"],"labels":["Trait reliant A et B"]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>geometry</dct:subject>
        <dct:educationLevel>1</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>101</dct:alternative>
    </rdf:Description>

