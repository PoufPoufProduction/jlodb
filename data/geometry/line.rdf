    <!-- SECANTE, PARALELLE AND PERPENDICULAR -->

     <rdf:Description>
        <dct:identifier>301</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser la barre d'outils pour construire une droite parallèle.</dct:title>
        <dct:description><![CDATA["data":[{"type":"circle", "value":[-50,-26,20],"attr":{"style":"stroke:#dd8833;"}},{"type":"segment", "value":[-50,-26,0,50],"attr":{"style":"stroke:#dd8833;"}},{"type":"segment", "value":[0,20,0,50],"attr":{"style":"stroke:#dd8833;"}},{"type":"segment", "value":[-28,40,0,50],"attr":{"style":"stroke:#dd8833;"}},{"type":"line","value":[0,-50,20,-60],"active":true},{"type":"point", "value":[0,50], "id":"A", "active":true,"idpos":315}],"available":["parallel"],"number":1,"objectives":[[{"type":"line", "values":[50,-26.57]}]],"translate":[320,185.5]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"statement":["Construisez une parallèle à la droite passant par A.","Sélectionnez l'outil [red][b]droite parallèle[/b][/red], cliquez sur la droite et en maintenant appuyé, dirigez vous vers le point A puis relachez.","[strong]Par définition, deux droites parallèles sont deux droites qui ne se coupent pas et n'ont donc pas d'intersection.[/strong]"],"labels":["Droite parallèle"]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>geometry</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
