     <rdf:Description>
        <dct:identifier>311</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser la barre d'outils pour définir le point d'intersection de 2 droites.</dct:title>
        <dct:description><![CDATA["data":[{"type":"circle", "value":[-50,-26,20],"attr":{"style":"stroke:#dd8833;"}},{"type":"segment", "value":[-50,-26,-50,50],"attr":{"style":"stroke:#dd8833;"}},{"type":"segment", "value":[-25,25,-50,50],"attr":{"style":"stroke:#dd8833;"}},{"type":"segment", "value":[-75,25,-50,50],"attr":{"style":"stroke:#dd8833;"}},{"type":"line","value":[0,-50,20,-60],"active":true},{"type":"line", "value":[100,70,-100,50],"active":true}],"available":["intersection"],"number":1,"objectives":[[{"type":"point", "values":[-183.33,41.67]}]],"translate":[320,185.5]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"statement":["[strong]Par définition, deux droites non parallèles sont dites sécantes et ont un point d'intersection, c'est à dire qu'il existe un point unique appartenant simultanément aux deux droites.[/strong]","En utilisant l'outil d'[red][b]intersection[/b][/red] définissez le points d'intersection des deux droites."],"labels":["Point d'intersection des deux droites"]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>geometry</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>301</dct:alternative>
    </rdf:Description>

     <rdf:Description>
