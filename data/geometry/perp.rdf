    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Tracer une droite perpendiculaire passant par un point A.</dct:title>
        <dct:description><![CDATA["active":true,"data":[{"type":"circle", "value":[-30,-50,15],"attr":{"style":"stroke:#dd8833;"},"active":false},{"type":"segment", "value":[-30,-50,-50,45],"attr":{"style":"stroke:#dd8833;"},"active":false},{"type":"segment", "value":[-39,38,-50,45],"attr":{"style":"stroke:#dd8833;"},"active":false},{"type":"segment", "value":[-59,35,-50,45],"attr":{"style":"stroke:#dd8833;"},"active":false},{"type":"line", "value":[-50,0]},{"type":"point", "value":[-50,50], "id":"A","idpos":0}],"available":["perpendicular"],"number":1,"objectives":[[{"type":"line", "values":[-50,90]}]],"translate":[320,185.5]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"statement":["Sélectionnez dans le menu l'opérateur [red]DROITE PERPENDICULAIRE[/red], cliquez sur la droite, puis en maintenant la pression, déplacez-vous vers le point A. Relachez la pression pour tracer la [b]la droite perpendiculaire[/b] passant par A."],"labels":["Droite perpendiculaire passant par A."]]]></dct:description>
        <dct:extent>2</dct:extent>
