    <!-- SEGMENTS AND LINES l[.] -->

    <rdf:Description>
        <dct:identifier>l001</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser la barre d'outils pour construire un segment à partir de 2 points.</dct:title>
        <dct:description><![CDATA["data":[{"type":"point", "value":[-150,0], "id":"A", "active":true}, {"type":"point", "value":[150,0], "id":"B", "active":true}],"available":["segment"],"number":1,"objectives":[[{"type":"segment", "values":[-150,0,150,0]}]],"translate":[320,185.5]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"statement":["[strong]En géométrie, un segment est une portion de droite délimitée par deux points, appelés extrémités du segment. Un segment reliant deux points A et B est noté [AB] et représente la partie de la droite (AB) qui se situe 'entre' les points A et B. Intuitivement, un segment correspond à un fil tendu entre deux points.[/strong]","Cliquez sur l'outil <b>segment</b> puis reliez les points A et B"],"labels":["Segment [AB]"]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>geometry</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>l101</dct:alternative>
    </rdf:Description>

