    <rdf:Description>
        <dct:identifier>db1</dct:identifier>
        <dct:title xml:lang="fr-FR">Déterminer la divisibilité par 2 et 3 de nombres compris entre 1 et 100.</dct:title>
        <dct:description><![CDATA["gg":{"v":[[0,2],[0,3]],"r":[90,10]},"background":"res/img/background/landscape/c_gray05.svg","init":{"pos":[[215,28],[215,65],[215,102],[320,28],[320,65],[320,102],[425,28],[425,65],[425,102],[530,28],[530,65],[530,102]]},"same":[["a","b","c","d"],["e","f","g","h"],["i","j","k","l"]],"url":"desktop/puzzle/groups3-4.svg", "gen":"function($this,_s,_id){var ret={},val=[['a','b','c','d'],['e','f','g','h'],['i','j','k','l']],r=[0,0]; do { r[0]=Math.floor(Math.random()*_s.gg.v[0][0])+_s.gg.v[0][1]; r[1]=Math.floor(Math.random()*_s.gg.v[1][0])+_s.gg.v[1][1]; } while (r[0]==r[1]); for (var i=0; i<3; i++) { var c=[]; for (var j=_s.gg.r[1]; j<_s.gg.r[0]+_s.gg.r[1]; j++) { if (((i<2)&&(j%r[i]==0)&&(j%r[1-i]!=0))||((i==2)&&(j%r[0]==0)&&(j%r[1]==0))) { c.push(j); } } shuffle(c); for (var j=0; j<4; j++) { ret[val[i][j]]=c[j]; } } return {'values':ret,'txt':{'text1':'×'+r[0],'text2':'×'+r[1],'text3':'×'+r[0]+' ×'+r[1]}};}"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"[b]Classez[/b] les différents multiples dans les colonnes correspondantes."]]></dct:description>
        <dct:extent>2</dct:extent>
