        <dct:description><![CDATA["gg":true,"background":"res/img/background/landscape/bluecarpet02.svg","number":2,"init":{"pos":[[60,300],[210,300],[360,300],[510,300],[135,250],[285,250],[435,250],[585,250],[135,350],[285,350],[435,350],[585,350]],"area":[0,0,100,100]},"decoys":["dd01","dd02","dd03","dd04","dd05","dd06"],"scale":1.5,"url":"desktop/puzzle/list01.svg","gen":"function($this,_s,_id){var val=Math.floor(Math.random()*15)+6,ref=String.fromCharCode(65+val),ret={};for (var i=0;i<6;i++) { ret['txt0'+(i+1)]=ref; ret[(_s.gg?'p0':'d0')+(i+1)]=String.fromCharCode(65+Math.floor(Math.random()*val));ret[(_s.gg?'d0':'p0')+(i+1)]=String.fromCharCode(65+val+1+Math.floor(Math.random()*(26-val-1)));} return {'txt':ret} }","show":["t01"],"same":[["pp01","pp02","pp03","pp04","pp05","pp06"]]]]></dct:description>
