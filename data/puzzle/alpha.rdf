        <dct:identifier>g3</dct:identifier>
        <dct:title xml:lang="fr-FR">Regrouper 12 lettres dans l'intervalle correspondant selon l'ordre alphabétique.</dct:title>
        <dct:description><![CDATA["background":"res/img/background/landscape/grayboard01.svg","number":1,"init":{"pos":[[215,28],[215,65],[215,102],[320,28],[320,65],[320,102],[425,28],[425,65],[425,102],[530,28],[530,65],[530,102]]},"same":[["a","b","c","d"],["e","f","g","h"],["i","j","k","l"]],"url":"desktop/puzzle/groups3-4.svg","gen":"function($this,_s,_id){ var nb=4,m=25,l='',a=(nb+2)+Math.floor(Math.random()*(m-3*(nb+2))),range=[0,a,Math.floor(Math.random()*(m-a-2*(nb+2)))+a+nb+2,m],txt={},values={}; for (var i=0; i<range.length-1;i++){ txt['text'+(i+1)]='['+String.fromCharCode(65+range[i])+','+String.fromCharCode(65+range[i+1])+']'; for(var j=0;j<nb;j++) { var vv,cpt=0; do { vv = String.fromCharCode(65+Math.floor(Math.random()*(range[i+1]-range[i]-1))+range[i]+1);  cpt++; } while (l.indexOf(vv)!=-1 && cpt<10); values[String.fromCharCode(97 + j + i*nb)]=vv; l+=vv;  } } return {txt:txt, values:values};}","svgclass":"big"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Placez chaque lettre dans [b]son groupe correspondant[/b] suivant l'[red]ordre alphabétique[/red]."]]></dct:description>
