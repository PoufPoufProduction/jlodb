        <dct:description><![CDATA["gg":6,"number":2,"timg":"res/img/svginventoryicons/book/paper01.svg","background":"res/img/background/landscape/wallpaper02.svg","init":{"pos":[[425,60],[495,60],[565,60],[425,130],[495,130],[565,130],[425,200],[495,200],[565,200],[425,270],[495,270],[565,270]]},"url":"desktop/puzzle/word.svg","gen":"function($this,_s,_i){ var c=[],v={}; for (var i=0; i<26; i++) { c.push(i); c.sort(function() { return (Math.random()<0.5); }); } c.splice(_s.gg,26-_s.gg); c.sort(function(_a,_b) { return (_a>_b); }); for (var i=0;i<_s.gg;i++) { v['p'+(i<9?'0':'')+(i+1)]=String.fromCharCode(65+c[i]); } return { 'values':v, 'svgclass':'l'+_s.gg}; }"]]></dct:description>
