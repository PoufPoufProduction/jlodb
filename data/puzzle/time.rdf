    <rdf:Description>
        <dct:identifier>ha</dct:identifier>
        <dct:title xml:lang="fr-FR">Faire glisser dans les zones grises, les horloges analogiques correspondant aux différentes heures numériques [heures et minutes avec leurres].</dct:title>
        <dct:description><![CDATA["svgclass":"tinytt","number":2,"errratio":2,"gg":[0,12,0,12,1,1],"background":"res/img/background/landscape/c_gray02.svg","number":2,"init":{"pos":[[60,300],[210,300],[360,300],[510,300],[135,250],[285,250],[435,250],[585,250],[135,350],[285,350],[435,350],[585,350]],"area":[0,0,100,100]},"decoys":["dd01","dd02","dd03","dd04","dd05","dd06"],"scale":1.5,"url":"desktop/puzzle/list01.svg","gen":"function($this,_s,_id){ var txt={},tt=[], id=[]; for (var h=_s.gg[0]; h<_s.gg[1]; h++) for (var m=_s.gg[2]; m<_s.gg[3]; m++) { tt.push(h*100+m*5); } shuffle(tt); for (var i=0; i<12; i++) { var h=Math.floor(tt[i]/100),m=tt[i]%100; h=(h<10?'0':'')+h; m=(m<10?'0':'')+m; if (i<6) { txt['txt0'+(i+1)] = h+':'+m; txt['pi0'+(i+1)]='res/img/clock/clock'+h+m+'.svg'; txt['pb0'+(i+1)]='res/img/clock/swatch0'+_s.gg[5]+'.svg'; id.push('pp0'+(i+1)); } else if (_s.gg[4]) { txt['di0'+(i-5)]='res/img/clock/clock'+h+m+'.svg'; txt['db0'+(i-5)]='res/img/clock/swatch0'+_s.gg[5]+'.svg'; id.push('dd0'+(i-5)); } } return { 'id':id,'txt':txt } }","show":["t01"]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Faites glisser dans les zones grises, les horloges correspondant aux différentes [b]heures[/b]."]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>time</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>ha</dct:alternative>
        <dct:coverage xml:lang="fr-FR">CE1</dct:coverage>
        <dct:source>res/img/desktop/puzzle/list01.svg,res/img/background/landscape/c_gray02.svg,res/img/clock/*.svg</dct:source>
    </rdf:Description>
	
