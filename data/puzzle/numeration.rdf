        <dct:description><![CDATA["gg":{"nb":4,"pi":12,"step":[1],"m":[50],"dec":[1],"off":[50]},"background":"res/img/background/landscape/yellowboard02.svg","number":2,"gen":"function($this,_s,_id){ var s=_s.gg.step[_id%_s.gg.step.length],off=_s.gg.off[_id%_s.gg.off.length],dec=_s.gg.dec[_id%_s.gg.dec.length],nb=_s.gg.nb,pi=_s.gg.pi,m=_s.gg.m[_id%_s.gg.m.length],o=off+Math.floor(Math.random()*(m/s-21))*s,show=[],txt={},v=[],vv={},d=[],dd=[],count=0;for (var i=0;i<21;i++) { if ((i*s+o)%(s*5)==0) {show.push('s'+(i<9?'0':'')+(i+1));} if ((i*s+o)%(s*10)==0) {txt['t'+(i<9?'0':'')+(i+1)]=(i*s+o)/dec; }}; while(v.length<nb&&count<100){var a=Math.floor(Math.random()*19)+1,ok=((a*s+o)%(s*10)!=0); for (var i in v) { if (Math.abs(a-v[i])<4) { ok=false; }} if (ok){ v.push(a); } count++; }; count=0; while (d.length<(pi-v.length)&&count<100) { var a=Math.floor(Math.random()*19)+1, ok=((a*s+o)%(s*10)!=0); for (var i in v) { if (a==v[i]) { ok=false; }} for (var i in d) { if (a==d[i]) { ok=false; }} if (ok){ d.push(a); } count++; }; for (var i in v) { vv['p'+(v[i]<9?'0':'')+(v[i]+1)]=(v[i]*s+o)/dec; show.push('pa'+(v[i]<9?'0':'')+(v[i]+1));} for (var i in d) { vv['d'+(d[i]<9?'0':'')+(d[i]+1)]=(d[i]*s+o)/dec; dd.push('d'+(d[i]<9?'0':'')+(d[i]+1)); } return {'show':show,'txt':txt,'decoys':dd,'values':vv}; }","url":"desktop/puzzle/ruler02.svg","init":{"pos":[[110,45],[195,45],[280,45],[365,45],[450,45],[535,45],[110,130],[195,130],[280,130],[365,130],[450,130],[535,130]]},"same":[[]]]]></dct:description>
