        <dct:title xml:lang="fr-FR">Renseigner correctement une graduation inférieure à 50.</dct:title>
        <dct:description><![CDATA["number":2,"gen":"function(_id){ var step=[1,10,100,1000],s=step[0],nb=4,pi=6,m=50,o=Math.floor(Math.random()*(m/s-21))*s,show=[],txt={},v=[],vv={},d=[],dd=[],count=0;for (var i=0;i<21;i++) { if ((i*s+o)%(s*5)==0) {show.push('s'+(i<9?'0':'')+(i+1));} if ((i*s+o)%(s*10)==0) {txt['t'+(i<9?'0':'')+(i+1)]=i*s+o; }}; while(v.length<nb&&count<100){var a=Math.floor(Math.random()*19)+1,ok=((a*s+o)%(s*10)!=0); for (var i in v) { if (Math.abs(a-v[i])<4) { ok=false; }} if (ok){ v.push(a); } count++; }; count=0; while (d.length<(pi-v.length)&&count<100) { var a=Math.floor(Math.random()*19)+1, ok=((a*s+o)%(s*10)!=0); for (var i in v) { if (a==v[i]) { ok=false; }} for (var i in d) { if (a==d[i]) { ok=false; }} if (ok){ d.push(a); } count++; }; for (var i in v) { vv['p'+(v[i]<9?'0':'')+(v[i]+1)]=v[i]*s+o; show.push('pa'+(v[i]<9?'0':'')+(v[i]+1));} for (var i in d) { vv['d'+(d[i]<9?'0':'')+(d[i]+1)]=d[i]*s+o; dd.push('d'+(d[i]<9?'0':'')+(d[i]+1)); } return {'show':show,'txt':txt,'decoys':dd,'values':vv}; }","init":{"pos":[[110,45],[195,45],[280,45],[365,45],[450,45],[535,45]]},"url":"desktop/puzzle/ruler02.svg","same":[[]]]]></dct:description>
