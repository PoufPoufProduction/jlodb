        <dct:description><![CDATA["errratio":5,"delay":[4000,4000],"background":"res/img/background/landscape/landscape02.svg","url":"desktop/puzzle/ladder01.svg","init":{"pos":[[80,80],[80,150]]},"txt":{"ih0":"ext/noto/svg/emoji_u1f416.svg","ih1":"ext/noto/svg/emoji_u1f408.svg","ih2":"ext/noto/svg/emoji_u1f407.svg","ih3":"ext/noto/svg/emoji_u1f412.svg","ih4":"ext/noto/svg/emoji_u1f404.svg"},"gg":{"nb":2,"maxb":3,"minb":3,"help":true},"gen":"function($this,settings,_id) { var nb=settings.gg.nb,maxb=settings.gg.maxb,minb=settings.gg.minb,off=[0,1,1,0,0],img=[['ext/noto/svg/emoji_u1f416.svg','ext/noto/svg/emoji_u1f437.svg'],['ext/noto/svg/emoji_u1f408.svg','ext/noto/svg/emoji_u1f431.svg'],['ext/noto/svg/emoji_u1f407.svg','ext/noto/svg/emoji_u1f430.svg'],['ext/noto/svg/emoji_u1f412.svg','ext/noto/svg/emoji_u1f435.svg'],['ext/noto/svg/emoji_u1f404.svg','ext/noto/svg/emoji_u1f42e.svg']],b=[[1,3,5,7,11],[2,4,6,8,10],[1,3,5,7,11],[2,4,8,9,10]],bb=[[1,3,5,7,11],[1,2,3,4,5,6,7,8,10,11],[1,2,3,4,5,6,7,8,10,11],[1,2,3,4,5,7,8,9,10,11],[2,4,8,9,10]],branches={}; for (var i=0; i<nb-1;i++) { var nn=Math.floor(Math.random()*(maxb-minb+1))+minb; for (var j=0; j<nn; j++) { var bj,ii=i+off[nb-1]; do { bj=b[ii][Math.floor(Math.random()*b[ii].length)]; } while(branches[ii.toString()+bj]); branches[ii.toString()+bj]=true; } } var bret=[]; for (var i in branches) { bret.push('b'+i); } var pos=[], values={}, svg=(settings.gg.help?'help ':'')+'nb'+nb; for (var i=0; i<nb; i++) {var x=i+off[nb-1],y=0; do { var newy=12; for (var j in bb[x]) { if (bb[x][j]>y && bb[x][j]<newy) { newy=bb[x][j]; } } bret.push((i+off[nb-1]).toString()+'v'+x.toString()+y.toString()+newy); y=newy; if (branches[(x-1).toString()+y]) { x=x-1; bret.push((i+off[nb-1]).toString()+'h'+x.toString()+y);} else if (branches[x.toString()+y]) { bret.push((i+off[nb-1]).toString()+'h'+x.toString()+y); x=x+1; } } while(y<12); values['pi'+x]=img[i+off[nb-1]][1]; svg+=' pc'+x.toString()+(i+off[nb-1]); } return { 'show':bret,'svgclass':svg,'values':values}; }","scale":1]]></dct:description>
