        <dct:title xml:lang="fr-FR">Identifier les angles droits d'un polygône.</dct:title>
        <dct:description><![CDATA["gg":{"nb":5,"max":[20,20]},"background":"res/img/background/landscape/c_gray02.svg","withrottimer":false,"errratio":2,"init":{"id":{"ruler":[460,180]},"area":[100,100,300,300]},"radius":120,"pieces":"tr01","decoys":["ruler"],"url":"desktop/puzzle/ruler03.svg","decoyfx":false,"gen":"function($this,_s,_id){ var ids=['ruler'],t=[]; for (var i=0; i<_s.gg.max[0]+_s.gg.max[1];i++) { t.push(i); } shuffle(t); for (var i=0; i<_s.gg.nb; i++) { var val=t[i],w=(val>=_s.gg.max[0]),v=val-(w?_s.gg.max[0]:0)+1; ids.push('t'+(w?'n':'a')+(v<10?'0':'')+v); } return {'id':ids }; }","cumul":true,"rotation":15,"show":["box"]]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"En [b]orientant[/b] correctement l'équerre par [red]simple clic[/red], [b]identifiez[/b] les différents triangles et [b]placez[/b]-les dans la boite correspondante.","fontex":0.7,"txt":{"box1":"Triangle rectangle","box2":"Triangle quelconque"}]]></dct:description>
