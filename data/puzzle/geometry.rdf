        <dct:description><![CDATA["show":["lred"],"gg":0,"background":"res/img/background/landscape/c_gray01.svg","desktop":[-320,-240],"withrottimer":false,"errratio":3,"init":{"id":{"ruler":[-220,-80]},"pos":[[-100,-150],[-50,-150],[0,-150]]},"radius":20,"decoys":["ruler"],"url":"desktop/puzzle/perpendicular01.svg","decoyfx":false,"gen":"function($this,_s,_id){ var ids=['ruler'],tr={'#mainrot':'rotate('+(4-Math.floor(Math.random()*9))+')'},t=[1,2,3,4,5,6]; shuffle(t); for (var i=0; i<6;i++) { if (i<3) { ids.push('pp0'+t[i]); tr['#l0'+t[i]]='rotate('+_s.gg+')'; } else { tr['#l0'+t[i]]='rotate('+(_s.gg+(1-2*Math.floor(Math.random()*2))*(2+Math.floor(Math.random()*5)))+')'; } } if (_s.gg) { tr['#rulerrot']='rotate('+_s.gg+')'; } return {'id':ids,'transform':tr }; }","cumul":true,"rotation":0,"magnetic":[[-255,140],[-150,145],[-55,140],[50,135],[155,145],[255,140]],"same":[["pp01","pp02","pp03","pp04","pp05","pp06"]]]]></dct:description>
