<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:dct="http://purl.org/dc/terms/">

    
    <rdf:Description>
        <dct:identifier>d2</dct:identifier>
        <dct:title xml:lang="fr-FR">Cliquer sur les nombres pairs compris entre 1 et 10.</dct:title>
        <dct:description><![CDATA["good":[{"type":"sig","value":"2"},{"type":"sig","value":"4"},{"type":"sig","value":"6"},{"type":"sig","value":"8"},{"type":"sig","value":"10"}],"wrong":[{"type":"sig","value":"1"},{"type":"sig","value":"3"},{"type":"sig","value":"5"},{"type":"sig","value":"7"},{"type":"sig","value":"9"}],"tag":"/2"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Cliquez sur les nombres [b]pairs[/b].[br]Un nombre [red]pair[/red] est un nombre divisible [red]en 2 moitiés égales[/red].","fontex":0.8]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>d2</dct:alternative>
        <dct:coverage xml:lang="fr-FR">CP</dct:coverage>
    </rdf:Description>
    
    <rdf:Description>
        <dct:identifier>d2a</dct:identifier>
        <dct:title xml:lang="fr-FR">Cliquer sur les nombres pairs compris entre 1 et 100.</dct:title>
        <dct:description><![CDATA["good":[{"type":"sig","gen":"function() { return Math.floor(Math.random()*49+1)*2;}"}],"wrong":[{"type":"sig","gen":"function() { return Math.floor(Math.random()*48+1)*2+1;}"}],"tag":"/2"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Cliquez sur les nombres [b]pairs[/b].[br]Un nombre [red]pair[/red] est un nombre dont le [red]chiffre des unités est pair[/red], c'est-à-dire un nombre [red]finissant[/red] par [b]0,2,4,6 ou 8[/b].","fontex":0.8]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>d2</dct:alternative>
        <dct:coverage xml:lang="fr-FR">CP</dct:coverage>
    </rdf:Description>
    

    
