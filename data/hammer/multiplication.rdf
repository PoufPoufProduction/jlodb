	<!-- CHECK -->
	
	
    <rdf:Description>
        <dct:identifier>c2</dct:identifier>
        <dct:title xml:lang="fr-FR">Cliquez sur les multiplications par 2 dont le résultat est exact.</dct:title>
        <dct:description><![CDATA["gg":[1,2,10,1],"freq":2,"class":"smalllegend","holes":"classic","sprdata":{"d":[{"alignment":"good","type":"embed","gen":"function($this,_s){var a=Math.floor(Math.random()*_s.gg[0])+_s.gg[1],b=Math.floor(Math.random()*_s.gg[2])+_s.gg[3]; return {legend:''+a+'×'+b+'='+(a*b)};}"},{"alignment":"wrong","type":"embed","gen":"function($this,_s){var a=Math.floor(Math.random()*_s.gg[0])+_s.gg[1],b=Math.floor(Math.random()*_s.gg[2])+_s.gg[3],c; do {c=Math.floor(Math.random()*((_s.gg[0]+_s.gg[1]-1)*(_s.gg[2]+_s.gg[3]-1)-(_s.gg[1]+_s.gg[3])))+_s.gg[1]+_s.gg[3];} while(c==a*b); return {legend:''+a+'×'+b+'='+c};}"}]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Cliquez sur les multiplications [b]exactes[/b]."]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>multiplication</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
