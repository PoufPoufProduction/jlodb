    <rdf:Description>
        <dct:identifier>3</dct:identifier>
        <dct:title xml:lang="fr-FR">Cliquer sur les valeurs divisibles par 3.</dct:title>
        <dct:description><![CDATA["good":[{"type":"sig","gen":"function() { return Math.floor(Math.random()*10+1)*3;}"}],"wrong":[{"type":"sig","gen":"function() { return Math.floor(Math.random()*9+1)*3+Math.floor(Math.random()*2+1);}"}],"tag":"×3"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Cliquez sur les valeurs [b]divisibles par 3[/b], c'est à dire dont la valeur appartient à la table de multiplication par 3."]]></dct:description>
